<?xml version="1.0" encoding="UTF-8"?>
<components xmlns:scr="http://www.osgi.org/xmlns/scr/v1.0.0">
    <scr:component name="org.girlscouts.cq.workflow.CustomProcessStep">
        <implementation class="org.girlscouts.cq.workflow.CustomProcessStep"/>
        <service servicefactory="false">
            <provide interface="com.adobe.granite.workflow.exec.WorkflowProcess"/>
        </service>
        <property name="process.label" value="Custom Email Process"/>
        <property name="service.pid" value="org.girlscouts.cq.workflow.CustomProcessStep"/>
        <reference name="messageGatewayService" interface="com.day.cq.mailer.MessageGatewayService" cardinality="1..1" policy="static" bind="bindMessageGatewayService" unbind="unbindMessageGatewayService"/>
    </scr:component>
</components>
