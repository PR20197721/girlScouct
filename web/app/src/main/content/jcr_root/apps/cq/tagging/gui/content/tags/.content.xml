<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <mergetag/>
    <jcr:content
        jcr:mixinTypes="[sling:VanityPath]"
        jcr:primaryType="nt:unstructured"
        jcr:title="AEM Tags"
        sling:resourceType="granite/ui/components/foundation/page"
        sling:vanityOrder="{Long}291"
        sling:vanityPath="/aem/tags">
        <head jcr:primaryType="nt:unstructured">
            <viewport
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/admin/page/viewport"/>
            <meta
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/meta"
                content="chrome=1"
                name="X-UA-Compatible"/>
            <favicon
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/page/favicon"/>
            <clientlibs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/includeclientlibs"
                categories="[jquery,jquery-ui,granite.ui.foundation,granite.ui.foundation.admin,granite.shared,cq.gui.common.admin.timeline,cq.gui.annotation,cq.tagging.touch.admin,cq.gui.siteadmin.activate,cq.gui.siteadmin.deactivate,cq.common.wcm,cq.siteadmin.admin.reorder]"/>
        </head>
        <body
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/page/body">
            <layout
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/endor/layouts/page"/>
            <rail
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/endor/navcolumns"
                currentId="cq-tools-tagging"
                srcPath="[cq/core/content/nav,granite/core/content/nav]"/>
            <title
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/hyperlink"
                href="#"
                text="Tags"/>
            <account
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/include"
                path="granite/ui/content/userproperties"/>
            <breadcrumbs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="cq/gui/components/siteadmin/admin/endor/breadcrumbs"
                baseUrl="/aem/tags.html"
                contentPath="${requestPathInfo.suffix}"
                depthAdjustment="{Long}-1"
                pageTitleTemplate="AEM TAGS | {{title}}"
                rootPageTitle="AEM TAGS"
                rootPath="/etc/tags"
                rootTitle="Tags"
                src="/aem/tags.html{+id}"
                target=".cq-tagadmin-admin-childpages"/>
            <helpactivator
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/hyperlink"
                href="#help-popover"
                text="Help"
                toggle="popover"/>
            <badge
                jcr:primaryType="nt:unstructured"
                sling:resourceType="cq/gui/components/endor/badge"
                href="#cq-notifications"/>
            <user
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/endor/user"
                href="#granite-user-properties"/>
            <help
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/container"
                id="help-popover">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/popover"/>
                <items jcr:primaryType="nt:unstructured">
                    <list
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <layout
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/layouts/list"/>
                        <items jcr:primaryType="nt:unstructured">
                            <searchform
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form"
                                action="http://www.adobe.com/cfusion/search/index.cfm">
                                <layout
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/layouts/form"/>
                                <items jcr:primaryType="nt:unstructured">
                                    <section
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/hidden"
                                        name="siteSection"
                                        value="marketing-cloud.html"/>
                                    <term
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        emptyText="Search"
                                        name="term"/>
                                </items>
                            </searchform>
                            <whatsnew
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/hyperlink"
                                href_i18n="https://microsite.omniture.com/t2/help/en_US/whatsnew/"
                                text="What's New">
                                <layoutConfig
                                    jcr:primaryType="nt:unstructured"
                                    interactive="{Boolean}true"/>
                            </whatsnew>
                            <home
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/hyperlink"
                                href_i18n="http://microsite.omniture.com/t2/help/en_US/home/"
                                text="Help Home">
                                <layoutConfig
                                    jcr:primaryType="nt:unstructured"
                                    interactive="{Boolean}true"/>
                            </home>
                            <community
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/hyperlink"
                                href_i18n="http://helpx.adobe.com/marketing-cloud.html"
                                text="Community">
                                <layoutConfig
                                    jcr:primaryType="nt:unstructured"
                                    interactive="{Boolean}true"/>
                            </community>
                            <customercare
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/hyperlink"
                                href_i18n="http://helpx.adobe.com/marketing-cloud/contact-support.html"
                                text="Customer Care">
                                <layoutConfig
                                    jcr:primaryType="nt:unstructured"
                                    interactive="{Boolean}true"/>
                            </customercare>
                        </items>
                    </list>
                </items>
            </help>
            <notifications
                jcr:primaryType="nt:unstructured"
                sling:resourceType="cq/gui/components/endor/notifications"
                id="cq-notifications"/>
            <content
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/endor/layouts/content"/>
                <aside
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/container">
                    <layout
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/endor/layouts/sidepanels"/>
                    <items jcr:primaryType="nt:unstructured">
                        <viewproperties
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/include"
                            id="aem-tags-rail-viewproperties"
                            path="cq/tagging/gui/content/commons/sidepanels/taginfo"/>
                        <references
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/include"
                            id="cq-rail-references"
                            path="cq/tagging/gui/content/commons/sidepanels/references"/>
                    </items>
                </aside>
                <header
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/container">
                    <layout
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/layouts/mode"
                        group="cq-tagadmin-admin-childpages"/>
                    <items jcr:primaryType="nt:unstructured">
                        <default
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/navigation">
                            <layout
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/endor/layouts/actionbar"/>
                            <items jcr:primaryType="nt:unstructured">
                                <createnamespace
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/endor/actionbar/hyperlink"
                                    hideText="{Boolean}true"
                                    href="#createnamespaceform"
                                    icon="icon-add"
                                    rel="cq-tagging-touch-actions-createnamespace-activator"
                                    text="Create Namespace"
                                    title="Create Namespace"
                                    toggle="modal">
                                    <rendercondition
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/renderconditions/privilege"
                                        path="${requestPathInfo.suffix}"
                                        privileges="[jcr:addChildNodes,jcr:modifyProperties]"/>
                                </createnamespace>
                            </items>
                        </default>
                        <selection
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/navigation">
                            <layout
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/endor/layouts/actionbar"/>
                            <items jcr:primaryType="nt:unstructured">
                                <viewproperties
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/endor/actionbar/innerrailtoggle"
                                    class="foundation-collection-action"
                                    foundation-collection-action="\{&quot;target&quot;: &quot;.cq-tagadmin-admin-childpages&quot;, &quot;activeSelectionCount&quot;: &quot;single&quot;}"
                                    icon="icon-info-circle"
                                    rel="cq-tagging-touch-actions-viewproperties-activator"
                                    targetId="aem-tags-rail-viewproperties"
                                    text="View Properties"
                                    title="View Properties"/>
                                <references
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/endor/actionbar/innerrailtoggle"
                                    class="foundation-collection-actions"
                                    foundation-collection-action="\{&quot;target&quot;: &quot;.cq-tagadmin-admin-childpages&quot;, &quot;activeSelectionCount&quot;: &quot;single&quot;}"
                                    icon="icon-unmerge"
                                    rel="cq-tagging-touch-actions-references-activator"
                                    targetId="cq-rail-references"
                                    text="References"
                                    title="References"/>
                                <createtag
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/endor/actionbar/hyperlink"
                                    activeSelectionCount="single"
                                    class="foundation-collection-action"
                                    foundation-collection-action="\{&quot;target&quot;: &quot;.cq-tagadmin-admin-childpages&quot;, &quot;activeSelectionCount&quot;: &quot;single&quot;}"
                                    hideText="{Boolean}true"
                                    href="#createtagform"
                                    icon="icon-add"
                                    rel="cq-tagging-touch-actions-createtag-activator"
                                    text="Create Tag"
                                    title="Create Tag"
                                    toggle="modal"/>
                                <edittag
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/endor/actionbar/button"
                                    append-selected-item-to="href"
                                    class="foundation-collection-action"
                                    foundation-collection-action="\{&quot;target&quot;: &quot;.cq-tagadmin-admin-childpages&quot;, &quot;activeSelectionCount&quot;: &quot;single&quot;}"
                                    hideText="{Boolean}true"
                                    href="/libs/cq/tagging/gui/content/tags/tagedit.html"
                                    icon="icon-edit"
                                    pageheading="AEM Tags| Tag Editor"
                                    rel="cq-tagging-touch-actions-edittag-activator"
                                    target=".cq-tagadmin-admin-childpages"
                                    text="Edit"
                                    title="Edit"/>
                                <movetag
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/endor/actionbar/button"
                                    append-selected-item-to="href"
                                    class="foundation-collection-action"
                                    foundation-collection-action="\{&quot;target&quot;: &quot;.cq-tagadmin-admin-childpages&quot;, &quot;activeSelectionCount&quot;: &quot;single&quot;}"
                                    hideText="{Boolean}true"
                                    href="/libs/cq/tagging/gui/content/tags/movetag.html"
                                    icon="icon-move"
                                    pageheading="Move Tags"
                                    rel="cq-tagging-touch-actions-movetag-activator"
                                    target=".cq-tagadmin-admin-childpages"
                                    text="Move"
                                    title="Move"/>
                                <mergetag
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/endor/actionbar/button"
                                    append-selected-item-to="href"
                                    class="foundation-collection-action"
                                    foundation-collection-action="\{&quot;target&quot;: &quot;.cq-tagadmin-admin-childpages&quot;, &quot;activeSelectionCount&quot;: &quot;single&quot;}"
                                    hideText="{Boolean}true"
                                    href="/libs/cq/tagging/gui/content/tags/mergetag.html"
                                    icon="icon-merge"
                                    pageheading="Merge Tags"
                                    rel="cq-tagging-touch-actions-mergetag-activator"
                                    target=".cq-tagadmin-admin-childpages"
                                    text="Merge"
                                    title="Merge"/>
                                <publishtag
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/endor/actionbar/hyperlink"
                                    class="foundation-collection-action"
                                    hideText="{Boolean}true"
                                    href="#publishtag"
                                    icon="icon-globe"
                                    rel="cq-tagging-touch-actions-publishtag-activator"
                                    text="Publish"
                                    title="Publish Tag"
                                    toggle="modal"/>
                                <unpublishtag
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/endor/actionbar/hyperlink"
                                    class="foundation-collection-action"
                                    foundation-collection-action="\{&quot;target&quot;: &quot;.cq-tagadmin-admin-childpages&quot;, &quot;activeSelectionCount&quot;: &quot;multiple&quot;}"
                                    hideText="{Boolean}true"
                                    href="#unpublishtag"
                                    icon="icon-globe-remove"
                                    rel="cq-tagging-touch-actions-unpublishtag-activator"
                                    text="Unpublish"
                                    title="Unpublish Tag"
                                    toggle="modal"/>
                                <deletetag
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/endor/actionbar/hyperlink"
                                    class="foundation-collection-action"
                                    foundation-collection-action="\{&quot;target&quot;: &quot;.cq-tagadmin-admin-childpages&quot;, &quot;activeSelectionCount&quot;: &quot;multiple&quot;}"
                                    hideText="{Boolean}true"
                                    href="#deletetag"
                                    icon="icon-delete"
                                    rel="cq-tagging-touch-actions-deletetag-activator"
                                    text="Delete"
                                    title="Delete Tag"
                                    toggle="modal"/>
                            </items>
                        </selection>
                    </items>
                </header>
                <content
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/container">
                    <items jcr:primaryType="nt:unstructured">
                        <childtags
                            granite:rel="cq-tagadmin-admin-childpages"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/container"
                            itemscope="itemscope">
                            <layout
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/layouts/multilayouts"
                                layout="${state[&quot;cq.tags.childtags.layoutId&quot;].string}">
                                <layouts jcr:primaryType="nt:unstructured">
                                    <columns
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/layouts/columnview"
                                        itemprop="item"
                                        itemResourceType="cq/tagging/gui/layouts/column/tagcolumnitem"
                                        layoutId="columns"
                                        limit="{Long}400"
                                        loadParents="{Boolean}true"
                                        multiselect="{Boolean}true"
                                        path="${requestPathInfo.suffix}"
                                        rootPath="/etc/tags"
                                        src="/libs/cq/tagging/gui/content/tags/miller/columnview{.offset,limit}.html{+path}"
                                        stateId="cq.tags.childtags"/>
                                </layouts>
                            </layout>
                            <datasource
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/tagging/gui/datasources/tagsdatasource"
                                com.adobe.cq.item.quickActions="true"
                                limit="${empty requestPathInfo.selectors[1] ? &quot;10&quot; : requestPathInfo.selectors[1]}"
                                offset="${requestPathInfo.selectors[0]}"/>
                            <granite:data
                                jcr:primaryType="nt:unstructured"
                                foundation-mode-group="cq-tagadmin-admin-childpages"/>
                        </childtags>
                        <createnamespace
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/contsys">
                            <items jcr:primaryType="nt:unstructured">
                                <includeclientlibs
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/includeclientlibs"
                                    categories="[cq.tagging.touch.createnamespace]"/>
                                <createnamespacemodal
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Add Namespace"
                                    sling:resourceType="granite/ui/components/foundation/form"
                                    enctype="application/x-www-form-urlencoded"
                                    id="createnamespaceform"
                                    method="POST">
                                    <layout
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/layouts/modal"
                                        closable="{Boolean}true"
                                        formStyle="vertical"
                                        type="default"/>
                                    <items jcr:primaryType="nt:unstructured">
                                        <title
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                                            fieldLabel="Title"
                                            id="namespacetitle"
                                            name="jcr:title"
                                            text="Enter namespace title"
                                            title="Enter namespace title"/>
                                        <name
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                                            fieldLabel="Name"
                                            id="namespacename"
                                            name="tag"
                                            text="Enter namespace name"
                                            title="Enter namespace name"/>
                                        <charset
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/hidden"
                                            ignoreData="{Boolean}true"
                                            name="_charset_"
                                            value="UTF-8"/>
                                        <description
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/textarea"
                                            fieldLabel="Description"
                                            id="namespacedescription"
                                            name="jcr:description"
                                            text="Enter namespace description"
                                            title="Enter namespace description"/>
                                        <command
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/hidden"
                                            ignoreData="{Boolean}true"
                                            name="cmd"
                                            value="createTag"/>
                                    </items>
                                    <footer jcr:primaryType="nt:unstructured">
                                        <close
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/button"
                                            text="Cancel">
                                            <layoutConfig
                                                jcr:primaryType="nt:unstructured"
                                                close="{Boolean}true"/>
                                        </close>
                                        <submit
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/button"
                                            id="createnamespace-submit"
                                            text="Create"
                                            variant="primary"/>
                                    </footer>
                                </createnamespacemodal>
                                <createnamespaceerrormodal
                                    jcr:description="Failed to create folder."
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Error"
                                    sling:resourceType="cq/tagging/gui/components/admin/modal"
                                    class="coral-Modal--error"
                                    dismiss-button-class="coral-Button coral-Button--primary"
                                    dismiss-button-text="Close"
                                    id="createnamespace-error"/>
                            </items>
                        </createnamespace>
                        <createtag
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/contsys">
                            <items jcr:primaryType="nt:unstructured">
                                <includeclientlibs
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/includeclientlibs"
                                    categories="[cq.tagging.touch.createtag]"/>
                                <createtagmodal
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Add Tag"
                                    sling:resourceType="granite/ui/components/foundation/form"
                                    enctype="application/x-www-form-urlencoded"
                                    id="createtagform"
                                    method="POST">
                                    <layout
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/layouts/modal"
                                        closable="{Boolean}true"
                                        formStyle="vertical"
                                        type="default"/>
                                    <items jcr:primaryType="nt:unstructured">
                                        <title
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                                            fieldLabel="Title"
                                            id="tagtitle"
                                            name="jcr:title"
                                            text="Enter tag title"
                                            title="Enter tag title"/>
                                        <name
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                                            fieldLabel="Name"
                                            id="tagname"
                                            name="tag"
                                            text="Enter tag name"
                                            title="Enter tag name"/>
                                        <charset
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/hidden"
                                            ignoreData="{Boolean}true"
                                            name="_charset_"
                                            value="UTF-8"/>
                                        <description
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/textarea"
                                            fieldLabel="Description"
                                            id="tagdescription"
                                            name="jcr:description"
                                            text="Enter tag description"
                                            title="Enter tag description"/>
                                        <command
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/hidden"
                                            ignoreData="{Boolean}true"
                                            name="cmd"
                                            value="createTag"/>
                                        <parenttagid
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/hidden"
                                            name="parentTagID"/>
                                    </items>
                                    <footer jcr:primaryType="nt:unstructured">
                                        <close
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/button"
                                            text="Cancel">
                                            <layoutConfig
                                                jcr:primaryType="nt:unstructured"
                                                close="{Boolean}true"/>
                                        </close>
                                        <submit
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/button"
                                            id="createtag-submit"
                                            text="Create"
                                            variant="primary"/>
                                    </footer>
                                </createtagmodal>
                                <createtagerrormodal
                                    jcr:description="Failed to create tag."
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Error"
                                    sling:resourceType="cq/tagging/gui/components/admin/modal"
                                    class="coral-Modal--error"
                                    dismiss-button-class="coral-Button coral-Button--primary"
                                    dismiss-button-text="Close"
                                    id="createtag-error"/>
                            </items>
                        </createtag>
                        <deletetag
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="cq/tagging/gui/components/deletetag"
                            id="deletetag"
                            rel="cq-tagging-touch-actions-delete"/>
                        <publishtag
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="cq/tagging/gui/components/publishtag"
                            id="publishtag"
                            rel="cq-tagging-touch-actions-publish"/>
                        <unpublishtag
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="cq/tagging/gui/components/unpublishtag"
                            id="unpublishtag"
                            rel="cq-tagging-touch-actions-unpublish"/>
                    </items>
                </content>
            </content>
        </body>
        <redirector
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/admin/contentchecker"
            allowed="[/etc/tags(.)*]"
            allowEmpty="{Boolean}false"
            path="${requestPathInfo.suffix}"
            redirect="/aem/tags.html/etc/tags"/>
    </jcr:content>
    <miller jcr:primaryType="nt:unstructured">
        <columnview
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/container">
            <layout
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/layouts/columnview"
                itemResourceType="cq/tagging/gui/layouts/column/tagcolumnitem"
                multiselect="{Boolean}true"
                src="/libs/cq/tagging/gui/content/tags/miller/columnview{.offset,limit}.html{+path}"/>
            <datasource
                jcr:primaryType="nt:unstructured"
                sling:resourceType="cq/tagging/gui/datasources/tagsdatasource"
                limit="${empty requestPathInfo.selectors[1] ? &quot;10&quot; : requestPathInfo.selectors[1]}"
                offset="${requestPathInfo.selectors[0]}"/>
        </columnview>
        <previewcolumn
            jcr:primaryType="nt:unstructured"
            sling:resourceType="cq/tagging/gui/layouts/column/tagcolumnpreview"
            path="${requestPathInfo.suffix}"/>
    </miller>
    <tagedit/>
    <movetag/>
</jcr:root>
