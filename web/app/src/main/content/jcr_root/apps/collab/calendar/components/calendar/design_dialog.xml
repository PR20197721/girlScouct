<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    title="Calendar (Design)"
    xtype="tabpanel">
    <items jcr:primaryType="cq:WidgetCollection">
        <lenses
            jcr:primaryType="cq:Panel"
            title="Lenses">
            <items jcr:primaryType="cq:WidgetCollection">
                <lensButtonStyle
                    jcr:primaryType="nt:unstructured"
                    fieldLabel="Lens Button Style"
                    name="./lenses/parbase/lensButtonStyle"
                    type="select"
                    value="icon"
                    xtype="selection">
                    <options jcr:primaryType="cq:WidgetCollection">
                        <icon
                            jcr:primaryType="nt:unstructured"
                            text="Icon"
                            value="icon"/>
                        <icontext
                            jcr:primaryType="nt:unstructured"
                            text="Icon &amp; Text"
                            value="icontext"/>
                    </options>
                </lensButtonStyle>
            </items>
        </lenses>
        <colors
            jcr:primaryType="cq:Panel"
            title="Colors">
            <items jcr:primaryType="cq:WidgetCollection">
                <colors
                    jcr:primaryType="cq:Widget"
                    fieldDescription="List of colors to use for calendars/subscriptions. The default calendar (events created in this calendar component) will get the first color, then the subscriptions follow."
                    fieldLabel="Calendar colors"
                    name="./colors"
                    orderable="{Boolean}true"
                    xtype="multifield">
                    <fieldConfig
                        jcr:primaryType="cq:Widget"
                        showHexValue="{Boolean}true"
                        xtype="colorfield"/>
                </colors>
            </items>
        </colors>
    </items>
</jcr:root>
