<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Survey Link">
                <items jcr:primaryType="cq:WidgetCollection">
                    <meetingId
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Meeting ID"
                        name="./meetingid"
                        xtype="textfield"/>
                    <surveyLink
                        jcr:primaryType="cq:Widget"
                        fieldLabel="URL"
                        name="./surveyLink"
                        xtype="textfield"/>
                    <bannerCopy
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Banner Copy"
                        name="./bannerCopy"
                        xtype="textfield"/>
                    <buttonCopy
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Button Copy"
                        name="./buttonCopy"
                        xtype="textfield"/>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>
