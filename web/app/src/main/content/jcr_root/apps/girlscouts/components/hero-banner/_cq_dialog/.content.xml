<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    width="62rem"
    height="60rem"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"
            type="nav"/>
        <items jcr:primaryType="nt:unstructured">
            <tab1
                jcr:primaryType="nt:unstructured"
                jcr:title="Slides"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/container"/>
                <items jcr:primaryType="nt:unstructured">
                    <container
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <slides
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType = "granite/ui/components/coral/foundation/form/multifield"
                                composite="true"
                                fieldDescription = "Click 'Add' to add a slide element"
                                fieldLabel = "Slider Elements">
                                <field
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType = "granite/ui/components/coral/foundation/container"
                                    class="coral-Table--bordered"
                                    name="./slides">
                                    <items jcr:primaryType="nt:unstructured">
                                        <slide
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="girlscouts/components/hero-banner-element">
                                            <layout
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/layouts/tabs"
                                                type="nav"/>
                                            <items jcr:primaryType="nt:unstructured">
                                                <regular-image
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Regular Image"
                                                        sling:resourceType="granite/ui/components/foundation/container">
                                                    <layout
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <columns
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/foundation/container">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <imagesize
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/foundation/form/hidden"
                                                                        name="regular/imagesize"
                                                                        value="regular"/>
                                                                <file
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                                        autoStart="{Boolean}false"
                                                                        class="cq-droptarget"
                                                                        fieldLabel="Image asset"
                                                                        fileNameParameter="fileName"
                                                                        fileReferenceParameter="fileReference"
                                                                        mimeTypes="[image]"
                                                                        multiple="{Boolean}false"
                                                                        name="regular/file"
                                                                        title="Upload Image Asset"
                                                                        uploadUrl="${suffix.path}"
                                                                        useHTML5="{Boolean}true"/>
                                                                <imageType
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/foundation/form/hidden"
                                                                        name="regular/sling:resourceType"
                                                                        value="foundation/components/image"/>
                                                                <video-url
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                                        fieldDescription="Vimeo or Youtube Video URL"
                                                                        fieldLabel="Video URL"
                                                                        name="regular/videoUrl"/>
                                                            </items>
                                                        </columns>
                                                    </items>
                                                </regular-image>
                                                <medium-screen-image
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Medium Screen Image"
                                                        sling:resourceType="granite/ui/components/foundation/container">
                                                    <layout
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <columns
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/foundation/container">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <imagesize
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/foundation/form/hidden"
                                                                        name="medium/imagesize"
                                                                        value="medium"/>
                                                                <file
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                                        autoStart="{Boolean}false"
                                                                        class="cq-droptarget"
                                                                        fieldLabel="Image asset"
                                                                        fileNameParameter="fileName"
                                                                        fileReferenceParameter="fileReference"
                                                                        mimeTypes="[image]"
                                                                        multiple="{Boolean}false"
                                                                        name="medium/file"
                                                                        title="Upload Image Asset"
                                                                        uploadUrl="${suffix.path}"
                                                                        useHTML5="{Boolean}true"/>
                                                                <imageType
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/foundation/form/hidden"
                                                                        name="medium/sling:resourceType"
                                                                        value="foundation/components/image"/>
                                                                <video-url
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                                        fieldDescription="Vimeo or Youtube Video URL"
                                                                        fieldLabel="Video URL"
                                                                        name="medium/videoUrl"/>
                                                            </items>
                                                        </columns>
                                                    </items>
                                                </medium-screen-image>
                                                <small-screen-image
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Small Screen Image"
                                                        sling:resourceType="granite/ui/components/foundation/container">
                                                    <layout
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <columns
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/foundation/container">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <imagesize
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/foundation/form/hidden"
                                                                        name="small/imagesize"
                                                                        value="small"/>
                                                                <file
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                                        autoStart="{Boolean}false"
                                                                        class="cq-droptarget"
                                                                        fieldLabel="Image asset"
                                                                        fileNameParameter="fileName"
                                                                        fileReferenceParameter="fileReference"
                                                                        mimeTypes="[image]"
                                                                        multiple="{Boolean}false"
                                                                        name="small/file"
                                                                        title="Upload Image Asset"
                                                                        uploadUrl="${suffix.path}"
                                                                        useHTML5="{Boolean}true"/>
                                                                <imageType
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/foundation/form/hidden"
                                                                        name="small/sling:resourceType"
                                                                        value="foundation/components/image"/>
                                                                <video-url
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                                        fieldDescription="Vimeo or Youtube Video URL"
                                                                        fieldLabel="Video URL"
                                                                        name="small/videoUrl"/>
                                                            </items>
                                                        </columns>
                                                    </items>
                                                </small-screen-image>
                                                <advanced
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Advanced"
                                                        sling:resourceType="granite/ui/components/foundation/container">
                                                    <layout
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <columns
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/foundation/container">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <alt
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                                        fieldDescription="(Leave empty to use the title defined above)"
                                                                        fieldLabel="Alt Text"
                                                                        name="alt"/>
                                                                <linkURL
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="girlscouts/components/form/pathfield"
                                                                        fieldDescription="Drop files or pages from the Content Finder"
                                                                        fieldLabel="Link to"
                                                                        name="linkURL"/>
                                                                <newWindow
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                                        checked="{Boolean}true"
                                                                        name="newWindow"
                                                                        text="Open In New Window"
                                                                        value="{Boolean}true"
                                                                        uncheckedValue="{Boolean}false"/>
                                                                <newwindow-delete
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/foundation/form/hidden"
                                                                        name="newWindow@Delete"
                                                                        value="{Boolean}true"/>
                                                                <newwindow-type
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/foundation/form/hidden"
                                                                        name="isHidden@TypeHint"
                                                                        value="Boolean"/>
                                                                <regular-size
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="cq/gui/components/authoring/dialog/sizefield"
                                                                        fieldLabel="Regular Size">
                                                                    <width
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceType="granite/ui/components/foundation/form/numberfield"
                                                                            name="regular/width"/>
                                                                    <height
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceType="granite/ui/components/foundation/form/numberfield"
                                                                            name="regular/height"/>
                                                                </regular-size>
                                                                <medium-screen-size
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="cq/gui/components/authoring/dialog/sizefield"
                                                                        fieldLabel="Medium Screen Size">
                                                                    <width
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceType="granite/ui/components/foundation/form/numberfield"
                                                                            name="medium/width"/>
                                                                    <height
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceType="granite/ui/components/foundation/form/numberfield"
                                                                            name="medium/height"/>
                                                                </medium-screen-size>
                                                                <small-screen-size
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="cq/gui/components/authoring/dialog/sizefield"
                                                                        fieldLabel="Small Screen Size">
                                                                    <width
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceType="granite/ui/components/foundation/form/numberfield"
                                                                            name="small/width"/>
                                                                    <height
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceType="granite/ui/components/foundation/form/numberfield"
                                                                            name="small/height"/>
                                                                </small-screen-size>
                                                            </items>
                                                        </columns>
                                                    </items>
                                                </advanced>
                                            </items>
                                        </slide>
                                    </items>
                                </field>
                            </slides>
                        </items>
                    </container>
                </items>
            </tab1>
            <tab2
                jcr:primaryType="nt:unstructured"
                jcr:title="Misc"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <slideshow-timer
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="(Provide time in millisecond.Default (6000ms))"
                                fieldLabel="Time Interval"
                                name="./slideshowtimer"/>
                            <sb-placement
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/select"
                                fieldDescription="(Select spring board location)"
                                fieldLabel="Spring Board Position"
                                name="./spplacement"
                                value="0">
                                <items jcr:primaryType="nt:unstructured">
                                    <none
                                        jcr:primaryType="nt:unstructured"
                                        text="None"
                                        value="none"/>
                                    <right
                                        jcr:primaryType="nt:unstructured"
                                        text="Right"
                                        value="right"/>
                                    <bottom
                                        jcr:primaryType="nt:unstructured"
                                        text="Bottom"
                                        value="bottom"/>
                                </items>
                            </sb-placement>
                        </items>
                    </columns>
                </items>
            </tab2>
            <first-spring-board
                jcr:primaryType="nt:unstructured"
                jcr:title="SprintBoard 1"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <first-sb-title
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Spring Board Title"
                                name="./firstsbtitle"/>
                            <first-sb-des
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textarea"
                                fieldLabel="Spring Board Details"
                                name="./firstsbdesc"/>
                            <first-sb-link
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="girlscouts/components/form/pathfield"
                                fieldLabel="Spring Board Url"
                                name="./firstsburl"/>
                            <first-sb-new-window
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                name="./firstsbnewwindow"
                                text="New Window"
                                value="{Boolean}true"
                                uncheckedValue="{Boolean}false"/>
                            <first-sb-button-txt
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Spring Board Button"
                                name="./firstsbbutton"/>
                        </items>
                    </columns>
                </items>
            </first-spring-board>
            <second-spring-board
                jcr:primaryType="nt:unstructured"
                jcr:title="Spring Board 2"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <second-sb-title
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Spring Board Title"
                                name="./secondsbtitle"/>
                            <second-sb-des
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textarea"
                                fieldLabel="Spring Board Details"
                                name="./secondsbdesc"/>
                            <second-sb-link
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="girlscouts/components/form/pathfield"
                                fieldLabel="Spring Board Url"
                                name="./secondsburl"/>
                            <second-sb-new-window
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                name="./secondsbnewwindow"
                                text="New Window"
                                value="{Boolean}true"
                                uncheckedValue="{Boolean}false"/>
                            <second-sb-button-txt
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Spring Board Button"
                                name="secondsbbutton"/>
                        </items>
                    </columns>
                </items>
            </second-spring-board>
        </items>
    </content>
</jcr:root>
