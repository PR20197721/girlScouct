<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container"
        class="cq-dialog-content-page">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                jcr:title="Page Properties"
                sling:resourceType="granite/ui/components/foundation/container"
                rel="cq-siteadmin-admin-properties-tabs">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/tabs"
                    type="nav"/>
                <items
                    jcr:primaryType="nt:unstructured"
                    sling:hideChildren="[personalization,cloudservices]">
                    <basic
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Title"
                        sling:resourceType="granite/ui/components/foundation/section">
                        <layout
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                            margin="{Boolean}false"/>
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <title
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Title"
                                        sling:resourceType="granite/ui/components/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <title
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                fieldLabel="Title"
                                                name="./jcr:title"/>
                                            <tags
                                                cq:hideOnCreate="{Boolean}true"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/common/tagspicker"
                                                fieldLabel="Tags/Keywords"
                                                multiple="{Boolean}true"
                                                name="./cq:tags"
                                                rootPath="/etc/tags"/>
                                            <pagename
                                                cq:hideOnEdit="{Boolean}true"
                                                cq:showOnCreate="{Boolean}true"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                fieldDescription="This field can be left empty to have the page name generate for the title field. This field does not accept any special characters such as spaces and ampersands. Replace them with &quot;-&quot; characters and use lowercase. For example for a page titled &quot;Forms &amp; Documents&quot; enter &quot;forms-and-documents&quot; or &quot;forms-documents&quot;"
                                                fieldLabel="Name"
                                                name="pageName"
                                                validation="admin.pagename"/>
                                            <seoTitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                fieldDescription="The page title that is showed in the browser title bar."
                                                fieldLabel="SEO Title"
                                                name="./seoTitle"/>
                                            <hideinnav
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                                name="./hideInNav"
                                                text="Hide in Navigation"
                                                uncheckedValue="{Boolean}false"
                                                value="{Boolean}true"/>
                                            <hideinnav-delete
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/hidden"
                                                name="./hideInNav@Delete"
                                                value="{Boolean}true"/>
                                            <hideinsearch
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                                name="./hideInSearch"
                                                text="Hide in Search"
                                                uncheckedValue="{Boolean}false"
                                                value="{Boolean}true"/>
                                            <hideinsearch-delete
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/hidden"
                                                name="./hideInSearch@Delete"
                                                value="{Boolean}true"/>
                                            <breakinheritance
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                                fieldDescription="Rejects all GSUSA content rollouts for this page. If you want to sync individual components with national content, you will need to unlock and relock them"
                                                name="./breakInheritance"
                                                text="Block Rollouts for this page"
                                                uncheckedValue="{Boolean}false"
                                                value="{Boolean}true"/>
                                            <breakinheritance-delete
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/hidden"
                                                name="./breakInheritance@Delete"
                                                value="{Boolean}true"/>
                                        </items>
                                    </title>
                                    <moretitles
                                        cq:showOnCreate="{Boolean}false"
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="More Titles and Description"
                                        sling:resourceType="granite/ui/components/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <pagetitle
                                                cq:showOnCreate="{Boolean}false"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                allowBulkEdit="{Boolean}true"
                                                cq-msm-lockable="pageTitle"
                                                fieldLabel="Page Title"
                                                name="./pageTitle"
                                                renderReadOnly="{Boolean}true"/>
                                            <navigationtitle
                                                cq:showOnCreate="{Boolean}false"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                allowBulkEdit="{Boolean}true"
                                                cq-msm-lockable="navTitle"
                                                fieldLabel="Navigation Title"
                                                name="./navTitle"
                                                renderReadOnly="{Boolean}true"/>
                                            <subtitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                allowBulkEdit="{Boolean}true"
                                                cq-msm-lockable="subtitle"
                                                fieldLabel="Subtitle"
                                                name="./subtitle"
                                                renderReadOnly="{Boolean}true"/>
                                            <description
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/textarea"
                                                allowBulkEdit="{Boolean}true"
                                                cq-msm-lockable="jcr:description"
                                                fieldLabel="Description"
                                                name="./jcr:description"
                                                renderReadOnly="{Boolean}true"/>
                                        </items>
                                    </moretitles>
                                    <vanityurl
                                        cq:showOnCreate="{Boolean}false"
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Vanity URL"
                                        sling:resourceType="granite/ui/components/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <vanitypath
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/multifield"
                                                cq-msm-lockable="sling:vanityPath"
                                                fieldDescription="The vanity url format is /content/councilpath/vaintypath Replace councilname with your council path and the vanitypath with the desired vanity url. For example for Girlscouts North East Texas a join vanity url is /content/gsnetx/join"
                                                fieldLabel="Vanity URL"
                                                renderReadOnly="{Boolean}true">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    name="./sling:vanityPath"
                                                    required="{Boolean}true"/>
                                            </vanitypath>
                                            <redirectVanityURL
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                                cq-msm-lockable="./sling:redirect"
                                                name="./sling:redirect"
                                                renderReadOnly="{Boolean}true"
                                                text="Redirect Vanity URL"
                                                uncheckedValue="{Boolean}false"
                                                value="{Boolean}true"/>
                                        </items>
                                    </vanityurl>
                                </items>
                            </column>
                        </items>
                    </basic>
                    <girl-scouts
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Girl Scouts"
                            sling:resourceType="granite/ui/components/foundation/section">
                        <layout
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                                margin="{Boolean}false"/>
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <socialmedia
                                            cq:showOnCreate="{Boolean}false"
                                            jcr:primaryType="nt:unstructured"
                                            jcr:title="Social Media"
                                            sling:resourceType="granite/ui/components/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <ogtitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Default: SEO Title"
                                                fieldLabel="og:title"
                                                name="./ogTitle"/>
                                            <ogsitename
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                defaultValue="Girl Scouts of the USA"
                                                fieldDescription="Default: Girl Scouts of the USA"
                                                fieldLabel="og:site_name"
                                                name="./ogSiteName"
                                                value="Girl Scouts of the USA"/>
                                            <ogurl
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="og:url"
                                                name="./ogUrl"/>
                                            <ogdescription
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="og:description"
                                                name="./ogDescription"/>
                                            <ogimage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="girlscouts/components/form/pathfield"
                                                fieldLabel="og:image"
                                                name="./ogImage"
                                                rootPath="/content/dam"/>
                                            <fbappid
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="fb:app_id"
                                                name="./fbAppId"/>
                                            <canonicalUrl
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="girlscouts/components/form/pathfield"
                                                fieldLabel="Canonical URL"
                                                name="./canonicalUrl"
                                                rootPath="/content"/>
                                        </items>
                                    </socialmedia>
                                </items>
                            </column>
                        </items>
                    </girl-scouts>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
