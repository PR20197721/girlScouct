<form action="#" method="post" >
<input type="hidden" name="action" value="check"/>
<h4>Content Path:</h4>
<input type="text" name="path" size=45 required>
<span style="color:grey">(e.g. /content/dam )</span>
<br>
<h4>Publish Server URL:</h4>
<input name="url" type="text" size=45  value="http://" required>
<span style="color:grey">(e.g. http://localhost:4503)</span><br>
<br>

<input type="submit" value="Submit">
</form>


