<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    activeTab="0"
    xtype="tabpanel">
    <items jcr:primaryType="cq:WidgetCollection">
        <tab1
            jcr:primaryType="cq:Panel"
            title="Milestone">
            <items jcr:primaryType="cq:WidgetCollection">
                <title
                    jcr:primaryType="cq:Widget"
                    fieldLabel="Milestone Blurb"
                    name="./jcr:content/blurb"/>
                <date
                    jcr:primaryType="cq:Widget"
                    fieldLabel="Date"
                    hideTime="{Boolean}true"
                    name="./jcr:content/date"
                    xtype="datetime"/>
                <show
                    jcr:primaryType="cq:Widget"
                    name="./jcr:content/show"
                    value="{Boolean}true"
                    xtype="hidden"/>
                <show-hint
                    jcr:primaryType="cq:Widget"
                    name="./jcr:content/show@TypeHint"
                    value="Boolean"
                    xtype="hidden"/>
                <ocm-classname
                    jcr:primaryType="cq:Widget"
                    fieldLabel="Milestone Blurb"
                    hidden="{Boolean}true"
                    name="./jcr:content/ocm_classname"
                    value="org.girlscouts.vtk.models.Milestone"
                    xtype="textfield"/>
            </items>
        </tab1>
    </items>
</jcr:root>
