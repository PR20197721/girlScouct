<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Tab 1">
                <items jcr:primaryType="cq:WidgetCollection">
                    <children
                        jcr:primaryType="cq:Widget"
                        fieldDescription="Enter the titles of the Accordions you would like. Please don't use more than one accordion on a page unless necessary. Multiple accordions on a single page may cause issues."
                        fieldLabel="Titles"
                        name="./children"
                        xtype="multifield">
                        <titles
                            jcr:primaryType="nt:unstructured"
                            xtype="textfield"/>
                    </children>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>
