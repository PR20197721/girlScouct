<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:description="A page with three columns -- the default Girl Scout page construction."
    jcr:primaryType="cq:Template"
    jcr:title="Three Column Page"
    allowedPaths="[/content(/.*)?]"
    ranking="{Long}10">
    <jcr:content
        jcr:primaryType="cq:PageContent"
        sling:resourceType="girlscouts/components/three-column-page">
            <content
            jcr:primaryType="nt:unstructured">
                <right
                    jcr:primaryType="nt:unstructured">
                    <right-rail-banners
                        jcr:primaryType="nt:unstructured"
                        customized="false"
                        sling:resourceType="girlscouts/components/right-rail-banners"/>
                </right>
            </content>
    </jcr:content>
</jcr:root>
