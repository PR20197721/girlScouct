<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Video HTML">
                <items jcr:primaryType="cq:WidgetCollection">
                    <name
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Name"
                        name="./name"
                        xtype="textfield"/>
                    <description
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Description"
                        name="./desc"
                        xtype="textfield"/>
                    <url
                        jcr:primaryType="cq:Widget"
                        fieldLabel="URL"
                        name="./url"
                        xtype="textfield"/>
                    <metingId
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Meeting ID"
                        name="./meetingid"
                        xtype="textfield"/>
                    <tags
                        jcr:primaryType="cq:Widget"
                        defaultValue="NASA"
                        fieldLabel="Tag"
                        name="./tag"
                        xtype="textfield"/>
                    <flag
                        jcr:primaryType="cq:Widget"
                        defaultValue="VIDEO"
                        fieldLabel="Flag"
                        name="./flag"
                        xtype="textfield"/>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>
