<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"
            type="nav"/>
        <items jcr:primaryType="nt:unstructured">
            <links
                jcr:primaryType="nt:unstructured"
                jcr:title="Links"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <children
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                composite="true"
                                fieldDescription="Click Add to add a link to eyebrow nav"
                                fieldLabel="Links">
                                <field
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                    class="coral-Table--bordered"
                                    name="./links">
                                    <items jcr:primaryType="nt:unstructured">
                                        <title
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                                            fieldLabel="Link Title"
                                            name="linkTitle"/>
                                        <url jcr:primaryType="nt:unstructured"
			                             	sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
			                                fieldLabel="Link"
			                                name="url"/>
                                        <new-window
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                            fieldDescription="Check the box to open link in new window"
                                            name="newWindow"
                                            text="New Window"
                                            value="{Boolean}true"/>
                                        <new-window-delete
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/hidden"
                                            name="newWindow@Delete"
                                            value="{Boolean}true"/>
                                        <supress
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                            fieldDescription="Suppress submenu in hamburger mobile menu"
                                            name="suppress"
                                            text="Supress Sub-menu in Hamburger"
                                            value="{Boolean}true"/>
                                        <supress-delete
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/hidden"
                                            name="suppress@Delete"
                                            value="{Boolean}true"/>
                                        <class
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                                            fieldDescription="Put CSS class here to add to link"
                                            fieldLabel="Class"
                                            name="class"/>
                                    </items>
                                </field>
                            </children>
                        </items>
                    </columns>
                </items>
            </links>
        </items>
    </content>
</jcr:root>
