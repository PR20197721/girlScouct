<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Page"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[cq.common.wcm,cq.siteadmin.admin.properties]"
    mode="edit">
    <content jcr:primaryType="nt:unstructured" >
        <items jcr:primaryType="nt:unstructured">
            <tabs jcr:primaryType="nt:unstructured" >
                <items jcr:primaryType="nt:unstructured">
                    <basic jcr:primaryType="nt:unstructured" >
                        <items jcr:primaryType="nt:unstructured">
                            <column jcr:primaryType="nt:unstructured">
                                <items jcr:primaryType="nt:unstructured">                                    
                                    <moretitles                                        
                                        jcr:primaryType="nt:unstructured"
                                        sling:hideResource="true"/>
                                    <onofftime                                        
                                        jcr:primaryType="nt:unstructured"
                                        sling:hideResource="true"/>
                                    <vanityurl
                                        cq:showOnCreate="{Boolean}false"
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Vanity URL"
                                        sling:resourceType="granite/ui/components/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <vanitypath
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/multifield"
                                                cq-msm-lockable="sling:vanityPath"
                                                fieldLabel="Vanity URL"
                                                renderReadOnly="{Boolean}true">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    name="./sling:vanityPath"
                                                    required="{Boolean}true"/>
                                            </vanitypath>
                                            <redirectVanityURL
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                                cq-msm-lockable="./sling:redirect"
                                                name="./sling:redirect"
                                                renderReadOnly="{Boolean}true"
                                                text="Redirect Vanity URL"
                                                value="true"/>
                                        </items>
                                    </vanityurl>
                                    <redirecturl
                                        cq:showOnCreate="{Boolean}false"
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Redirect URL"
                                        sling:resourceType="granite/ui/components/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">                                            
	                                        <field
	                                            jcr:primaryType="nt:unstructured"
	                                            sling:resourceType="girlscouts/components/form/pathfield"
	                                            name="./redirectTarget"
	                                            required="{Boolean}true"/>                                            
                                        </items>
                                    </redirecturl>
                                </items>
                            </column>
                        </items>
                    </basic>
                    <advanced                                        
                        jcr:primaryType="nt:unstructured"
                        sling:hideResource="true"/>
                    <thumbnail                                        
                        jcr:primaryType="nt:unstructured"
                        sling:hideResource="true"/>
                    <cloudservices                                        
                        jcr:primaryType="nt:unstructured"
                        sling:hideResource="true"/>
                    <personalization                                        
                        jcr:primaryType="nt:unstructured"
                        sling:hideResource="true"/>
                    <permissions                                        
                        jcr:primaryType="nt:unstructured"
                        sling:hideResource="true"/>
                    <blueprint                                        
                        jcr:primaryType="nt:unstructured"
                        sling:hideResource="true"/>
                    <livecopy                                        
                        jcr:primaryType="nt:unstructured"
                        sling:hideResource="true"/>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
