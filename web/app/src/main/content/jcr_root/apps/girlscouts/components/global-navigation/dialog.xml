<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    warnIfModified="{Boolean}false"
    width="{Long}1000"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <first
                jcr:primaryType="cq:Widget"
                title="Multi-Field"
                xtype="panel">
                <items jcr:primaryType="cq:WidgetCollection">
                    <multi
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Links"
                        hideLabel="{Boolean}true"
                        name="./links"
                        xtype="multifield">
                        <fieldConfig
                            jcr:primaryType="nt:unstructured"
                            xtype="customgloballink"/>
                    </multi>
                </items>
            </first>
            <styles
                jcr:primaryType="cq:Widget"
                title="Styles"
                xtype="panel">
                <items jcr:primaryType="cq:WidgetCollection">
                    <additional-css
                        jcr:primaryType="cq:Widget"
                        fieldDescription="Separated by space. CSS tags will be added to the surrounding div tag when the component is being included."
                        fieldLabel="Additional CSS"
                        name="./additionalCss"
                        xtype="textfield"/>
                </items>
            </styles>
            <options
                jcr:primaryType="cq:Widget"
                title="Options"
                xtype="panel">
                <items jcr:primaryType="cq:WidgetCollection">
                	<display-nav-fly-out
                		jcr:primaryType="cq:Widget"
                        fieldDescription="Will create a secondary navigation flyout when user hovers over top nav link."
                        fieldLabel="Display Secondary Navigation Flyout"
                        name="./displaySecondaryNavFlyOut"
                        type = "checkbox"
                        xtype="selection"/>
                    <!--  
                    <display-page-banner
                        jcr:primaryType="cq:Widget"
                        fieldDescription="Will create a transparent banner area so a title can be added. Will be displayed between navigation and content."
                        fieldLabel="Display Transparent Page Banner"
                        name="./displayPageBanner"
                        type = "checkbox"
                        xtype="selection"/>		
                    <mobile-cta
			            jcr:primaryType="cq:Widget"
			            fieldLabel="Mobile Calls to Action"
			            fieldDescription="Class field will control button color. Available CSS classes: &lt;strong&gt;default(blank)&lt;/strong&gt; = #009447, &lt;strong&gt;orange-btn&lt;/strong&gt; = #faa61a, &lt;strong&gt;yellow-btn&lt;/strong&gt; = #f0c022"
			            name="./mobileCtas"
			            xtype="multifield">
			            <fieldConfig
			                jcr:primaryType="nt:unstructured"
			                xtype="customlink"/>
			        </mobile-cta>
			        -->
                </items>
            </options>
        </items>
    </items>
</jcr:root>
