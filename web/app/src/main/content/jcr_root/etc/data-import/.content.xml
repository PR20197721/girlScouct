<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured">
    <girlscouts jcr:primaryType="nt:unstructured">
        <csv jcr:primaryType="nt:unstructured">
            <Event
                jcr:primaryType="nt:unstructured"
                primaryType="cq:Page">
                <fields jcr:primaryType="nt:unstructured">
                    <title
                        jcr:primaryType="nt:unstructured"
                        name="jcr:title"
                        type="string"/>
                    <start
                        jcr:primaryType="nt:unstructured"
                        name="data/start"
                        type="date(yyyy/MM/dd h:mma)"/>
                    <end
                        jcr:primaryType="nt:unstructured"
                        name="data/end"
                        type="date(yyyy/MM/dd h:mma)"/>
                    <region
                        jcr:primaryType="nt:unstructured"
                        name="data/region"
                        type="string"/>
                    <locationLabel
                        jcr:primaryType="nt:unstructured"
                        name="data/locationLabel"
                        type="string"/>
                    <address
                        jcr:primaryType="nt:unstructured"
                        name="data/address"
                        type="string"/>
                    <details
                        jcr:primaryType="nt:unstructured"
                        name="data/details"
                        type="string"/>
                    <srchdisp
                        jcr:primaryType="nt:unstructured"
                        name="data/srchdisp"
                        type="string"/>
                    <color
                        jcr:primaryType="nt:unstructured"
                        name="data/color"
                        type="string"/>
                    <register
                        jcr:primaryType="nt:unstructured"
                        name="data/register"
                        type="string"/>
                    <program-level
                        jcr:primaryType="nt:unstructured"
                        name="cq:tags"
                        script="function(value) {String.prototype.trim=function(){return this.replace(/^s+|s+$/g, '');}; var tags = value.split(','); for (var i = 0; i &lt; tags.length; i++) {tags[i] = 'girlscouts:program-level/' + tags[i].trim().replace(/s+/g,'-').toLowerCase();} return tags.join(',');}"
                        type="string[]"/>
                    <category
                        jcr:primaryType="nt:unstructured"
                        name="cq:tags"
                        script="function(tag) {String.prototype.trim=function(){return this.replace(/^s+|s+$/g, '');}; return &quot;girlscouts:category/&quot; + tag.trim().replace(/ /g, &quot;-&quot;).toLowerCase();}"
                        type="string[]"/>
                </fields>
                <nameGen
                    jcr:primaryType="nt:unstructured"
                    script="function(title, date) {return date.substring(0, 4) + '/' + title.toLowerCase().replace(/ /g, '-'); }"
                    scriptFields="title,start"/>
                <defaultFields jcr:primaryType="nt:unstructured">
                    <sling:resourceType
                        jcr:primaryType="nt:unstructured"
                        type="string"
                        value="girlscouts/components/event-page"/>
                </defaultFields>
            </Event>
            <News
                jcr:primaryType="nt:unstructured"
                primaryType="cq:Page">
                <fields jcr:primaryType="nt:unstructured">
                    <title
                        jcr:primaryType="nt:unstructured"
                        name="jcr:title"
                        type="string"/>
                    <description
                        jcr:primaryType="nt:unstructured"
                        type="string"/>
                    <date
                        jcr:primaryType="nt:unstructured"
                        type="date(yyyy-MM-dd HH:mm)"/>
                </fields>
                <nameGen
                    jcr:primaryType="nt:unstructured"
                    script="function(title, date) {return date.substring(0, 4) + '/' + title.toLowerCase().replace(/ /g, '-'); }"
                    scriptFields="title,date"/>
                <defaultFields jcr:primaryType="nt:unstructured">
                    <sling:resourceType
                        jcr:primaryType="nt:unstructured"
                        type="string"
                        value="girlscouts/components/news"/>
                </defaultFields>
            </News>
            <Contact
                jcr:primaryType="nt:unstructured"
                primaryType="cq:Page">
                <fields jcr:primaryType="nt:unstructured">
                    <name
                        jcr:primaryType="nt:unstructured"
                        name="jcr:title"
                        type="string"/>
                    <title
                        jcr:primaryType="nt:unstructured"
                        name="whatever/whatever2/title"
                        type="string"/>
                    <phone
                        jcr:primaryType="nt:unstructured"
                        type="string"/>
                    <email
                        jcr:primaryType="nt:unstructured"
                        type="string"/>
                    <team
                        jcr:primaryType="nt:unstructured"
                        type="string"/>
                </fields>
                <nameGen
                    jcr:primaryType="nt:unstructured"
                    script="function(name, team) {return team.toLowerCase().replace(/ /g, '-') + '/' + name.toLowerCase().replace(/ /g, '-'); }"
                    scriptFields="name,team"/>
                <defaultFields jcr:primaryType="nt:unstructured"/>
                <parent-fields jcr:primaryType="nt:unstructured">
                    <team
                        jcr:primaryType="nt:unstructured"
                        name="jcr:title"/>
                </parent-fields>
            </Contact>
        </csv>
    </girlscouts>
</jcr:root>
