                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="girlscouts/components/textimage"
                        text="&lt;p>&lt;b>Looking for directions to camp?&lt;/b>&lt;/p>&#xa;"
                        textIsRich="true">
                        <image
                            jcr:lastModified="{Date}2014-08-25T10:39:56.989-04:00"
