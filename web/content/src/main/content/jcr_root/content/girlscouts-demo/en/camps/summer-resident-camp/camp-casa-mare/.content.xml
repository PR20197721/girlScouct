                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="girlscouts/components/textimage"
                        text="&lt;p>&lt;b>Accommodations&lt;/b>&lt;/p>&#xa;"
                        textIsRich="true">
                        <image
