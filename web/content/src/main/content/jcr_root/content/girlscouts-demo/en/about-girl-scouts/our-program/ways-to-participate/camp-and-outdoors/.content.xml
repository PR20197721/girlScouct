                        text="&lt;p style=&quot;text-align: center;&quot;>&lt;img src=&quot;/content/dam/all_icons/fire_blue_64.png&quot;>&lt;/p>&#xa;"
                        textIsRich="true"/>
                    <text_0
                        cq:lastRolledout="{Date}2014-08-20T12:16:20.170-04:00"
                        cq:lastRolledoutBy="mage"
                        jcr:created="{Date}2014-06-23T16:36:01.708-04:00"
