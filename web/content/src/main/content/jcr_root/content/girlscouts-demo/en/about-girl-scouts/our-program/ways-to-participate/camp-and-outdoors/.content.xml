                        jcr:lastModified="{Date}2014-08-20T12:16:20.170-04:00"
                        jcr:lastModifiedBy="ctruett"
                        jcr:mixinTypes="[cq:LiveRelationship]"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="girlscouts/components/text"
                        text="&lt;p>You’ll develop the kind of outdoor skills it’s hard to get anywhere else. Skills like building a campfire, pitching a tent, or &lt;b>canoeing&lt;/b> across a lake.&lt;/p>&#xa;"
                        textIsRich="true"/>
