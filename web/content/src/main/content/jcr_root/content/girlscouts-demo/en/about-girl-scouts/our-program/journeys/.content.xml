                        jcr:mixinTypes="[cq:LiveRelationship]"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="girlscouts/components/textimage"
                        additionalCss="nowrap nopadding"
                        text="&lt;p>Identify a problem you want to do something about&lt;br>&#xa;&amp;nbsp;&lt;/p>&#xa;"
                        textIsRich="true">
                        <image
