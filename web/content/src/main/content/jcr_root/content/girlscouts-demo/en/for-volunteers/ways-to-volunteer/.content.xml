                            imageRotate="0"/>
                    </textimage_4>
                    <textimage_5
                        cq:isCancelledForChildren="{Boolean}true"
                        cq:lastRolledout="{Date}2014-08-20T12:17:23.595-04:00"
                        cq:lastRolledoutBy="mage"
                        jcr:created="{Date}2014-06-25T11:36:09.089-04:00"
                        jcr:createdBy="ctruett"
                        jcr:lastModified="{Date}2014-09-09T14:53:32.765-04:00"
                        jcr:lastModifiedBy="lballanco"
                        jcr:mixinTypes="[cq:LiveSyncCancelled]"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="girlscouts/components/textimage"
                        additionalCss="nowrap nopadding"
                        text="&lt;p>&lt;b>Product Sales Volunteers&lt;br>&#xa; &lt;/b>Get involved with the cookie and fall product programs! Coordinate these annual programs at the troop level to help girls earn funds for activities. Set up cookie booths, manage inventory and money, and then watch the&amp;nbsp;girls light up when they earn the rewards for being the best&amp;nbsp;cookie and fall product sellers ever!&lt;br>&#xa;&lt;a adhocenable=&quot;false&quot; href=&quot;http://volunteer.gssjc.org&quot; target=&quot;_blank&quot;>Volunteer Now&lt;/a>&lt;/p>&#xa;&lt;p>&amp;nbsp;&lt;/p>&#xa;"
                        textIsRich="true">
