<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:oak="http://jackrabbit.apache.org/oak/ns/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:rep="internal"
    jcr:mixinTypes="[rep:AccessControllable]"
    jcr:primaryType="nt:unstructured">
    <cqCloudServiceConfig/>
    <cqMaster/>
    <workflowDataLucene/>
    <jcrLanguage/>
    <event.job.topic/>
    <extensionType/>
    <slingeventEventId/>
    <containeeInstanceId/>
    <active/>
    <processingProfile/>
    <counter/>
    <authorizables/>
    <cqDefaultFormFor/>
    <slingResourceSuperType/>
    <cqLastReplicationAction/>
    <cqCugEnabled/>
    <damScene7ID/>
    <cqAcUUID/>
    <campaignpath/>
    <subType/>
    <damS7watch/>
    <acPrincipalName/>
    <fpContentType/>
    <cqVariantFamily/>
    <uuid/>
    <slingVanityPath/>
    <jcrLockOwner/>
    <damAssetLucene/>
    <deviceIdentificationMode/>
    <refExpired/>
    <rep:policy/>
    <status/>
    <cqCloudServiceConfigs/>
    <slingResources/>
    <cqPayloadPath/>
    <type/>
    <contentPath/>
    <jcrFrozenMixinTypes/>
    <cqOwnerCanvasPage/>
    <slingResource/>
    <nodetype/>
    <cqTemplate/>
    <ntBaseLucene/>
    <verb/>
    <reference/>
    <cqLastRolledoutBy/>
    <cqParentPath/>
    <lockCreated/>
    <principalName/>
    <cqTagLucene/>
    <lucene/>
    <guideComponentType/>
    <slingAlias/>
    <damResolvedPath/>
    <authorizableId/>
    <cqPageLucene/>
    <slingResourceType/>
    <cqKeywords/>
    <damStatus/>
    <GSPropertyIndex
        jcr:primaryType="oak:QueryIndexDefinition"
        async="async"
        evaluatePathRestrictions="{Boolean}true"
        fulltextEnabled="{Boolean}false"
        propertyNames="{Name}[ocm_classname,cq:tags,region]"
        reindex="{Boolean}false"
        reindexCount="{Long}5"
        type="property"/>
    <GSLuceneFullTextIndex
        jcr:primaryType="oak:QueryIndexDefinition"
        async="async"
        reindex="{Boolean}false"
        reindexCount="{Long}1"
        type="lucene">
        <indexRules jcr:primaryType="nt:unstructured">
            <nt:base
                jcr:primaryType="nt:unstructured"
                includePropertyTypes="all">
                <properties jcr:primaryType="nt:unstructured">
                    <prop0
                        jcr:primaryType="nt:unstructured"
                        analyzed="{Boolean}true"
                        isRegexp="{Boolean}true"
                        name="^[^\\/]*$"
                        nodeScopeIndex="{Boolean}true"
                        propertyIndex="{Boolean}false"
                        useInExcerpt="{Boolean}true"/>
                </properties>
            </nt:base>
        </indexRules>
    </GSLuceneFullTextIndex>
    <GSLucenePropertyIndex
        jcr:primaryType="oak:QueryIndexDefinition"
        aggregates="[ocm_classname,jcr:path]"
        async="async"
        includedPaths="[/vtk2015]"
        reindex="{Boolean}false"
        reindexCount="{Long}3"
        type="lucene">
        <indexRules jcr:primaryType="nt:unstructured">
            <nt:base
                jcr:primaryType="nt:unstructured"
                includePropertyTypes="all">
                <properties jcr:primaryType="nt:unstructured">
                    <ocm_classname
                        jcr:primaryType="nt:unstructured"
                        name="ocm_classname"
                        propertyIndex="{Boolean}true"/>
                </properties>
            </nt:base>
        </indexRules>
    </GSLucenePropertyIndex>
</jcr:root>
