                        sling:resourceType="girlscouts/components/text"
                        text="&lt;p style=&quot;text-align: center;&quot;>&lt;a href=&quot;http://girlscouts.org/program/gs_cookies/&quot; adhocenable=&quot;false&quot; target=&quot;_blank&quot;>&lt;img src=&quot;/content/dam/girlscouts-shared/images/buttons/btn_find_cookies.png&quot;>&lt;/a>&lt;/p>&#xa;&lt;p style=&quot;text-align: center;&quot;>&lt;a href=&quot;http://girlscouts.org/program/gs_cookies/&quot; adhocenable=&quot;false&quot;>&amp;nbsp;&lt;/a>&lt;a href=&quot;http://girlscouts.org/program/gs_cookies/&quot; adhocenable=&quot;false&quot;>&lt;/a>&lt;/p>&#xa;"
                        textIsRich="true"/>
                    <text_4ed
                        jcr:created="{Date}2015-12-16T12:35:37.950-05:00"
                        jcr:createdBy="dpage"
                        jcr:lastModified="{Date}2015-12-16T12:35:45.007-05:00"
