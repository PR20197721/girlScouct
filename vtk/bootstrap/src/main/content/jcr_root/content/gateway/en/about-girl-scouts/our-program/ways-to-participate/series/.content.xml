                        initialSlingResourceType="girlscouts/components/text"
                        text="&lt;p style=&quot;text-align: center;&quot;>&amp;nbsp;&lt;/p>&#xa;&lt;p style=&quot;text-align: center;&quot;>&lt;img width=&quot;99&quot; height=&quot;98&quot; src=&quot;/content/dam/girlscouts-shared/template/images/about/BFFLogo3COLOR-CMYK-UC.png&quot;>&lt;/p>&#xa;"
                        textIsRich="true"/>
                    <text_0
                        cq:lastRolledout="{Date}2014-07-17T18:11:43.940-04:00"
                        cq:lastRolledoutBy="admin"
                        jcr:created="{Date}2014-05-23T15:38:38.600-04:00"
                        jcr:createdBy="kstavisky"
                        jcr:lastModified="{Date}2014-07-11T09:43:14.949-04:00"
