<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    warnIfModified="{Boolean}false"
    width="{Long}1000"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <first
                jcr:primaryType="cq:Widget"
                title="VTK Resources"
                xtype="panel">
                <items jcr:primaryType="cq:WidgetCollection">
                    <upload-button
                        jcr:primaryType="nt:unstructured"
                        xtype="vtkresourcesuploadbutton"/>
                    <notice
                        jcr:primaryType="nt:unstructured"
                        html="Resource lists are editable only by national authors."
                        xtype="label"/>
                    <title
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Title"
                        hideTrigger="{Boolean}true"
                        name="./title"
                        xtype="pathfield">
                        <listeners
                            jcr:primaryType="nt:unstructured"
                            beforerender="function(comp){girlscouts.components.hideCouncilWidget(comp)}"/>
                    </title>
                    <multi
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Links"
                        hideLabel="{Boolean}true"
                        name="./items"
                        xtype="multifield">
                        <fieldConfig
                            jcr:primaryType="nt:unstructured"
                            xtype="vtkresourcespopup"/>
                        <listeners
                            jcr:primaryType="nt:unstructured"
                            beforerender="function(comp){girlscouts.components.hideCouncilWidget(comp)}"/>
                    </multi>
                </items>
            </first>
        </items>
    </items>
</jcr:root>
