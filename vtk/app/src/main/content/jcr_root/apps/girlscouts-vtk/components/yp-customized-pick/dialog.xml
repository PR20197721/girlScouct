<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
          jcr:primaryType="cq:Dialog"
          height="{Long}450"
          xtype="dialog">
    <items
            jcr:primaryType="cq:Widget"
            xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <properties
                    jcr:primaryType="cq:Panel"
                    title="Modal">
                <items jcr:primaryType="cq:WidgetCollection">
                    <customizedName
                            jcr:primaryType="cq:Widget"
                            fieldLabel="Customized Year Plan Name"
                            name="./title"
                            xtype="textfield"/>
                    <linkText
                            jcr:primaryType="cq:Widget"
                            fieldLabel="Customized Year Plan Link Text"
                            name="./linkText"
                            xtype="textfield"/>
                </items>
            </properties>
        </items>
    </items>
</jcr:root>
