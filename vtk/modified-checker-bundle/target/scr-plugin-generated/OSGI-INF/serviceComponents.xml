<?xml version="1.0" encoding="UTF-8"?><components xmlns:scr="http://www.osgi.org/xmlns/scr/v1.0.0">
    <scr:component enabled="true" name="org.girlscouts.vtk.modifiedcheck.impl.ModifiedCheckImpl">
        <implementation class="org.girlscouts.vtk.modifiedcheck.impl.ModifiedCheckImpl"/>
        <service servicefactory="false">
            <provide interface="org.girlscouts.vtk.modifiedcheck.ModifiedChecker"/>
        </service>
        <property name="service.pid" value="org.girlscouts.vtk.modifiedcheck.impl.ModifiedCheckImpl"/>
    </scr:component>
    <scr:component enabled="true" name="org.girlscouts.vtk.modifiedcheck.impl.ModifiedCheckerServlet">
        <implementation class="org.girlscouts.vtk.modifiedcheck.impl.ModifiedCheckerServlet"/>
        <service servicefactory="false">
            <provide interface="javax.servlet.Servlet"/>
        </service>
        <property name="sling.servlet.resourceTypes" value="sling/servlet/default"/>
        <property name="sling.servlet.selectors" value="expiredcheck"/>
        <property name="sling.servlet.extensions" value="json"/>
        <property name="sling.servlet.methods" value="GET"/>
        <property name="service.pid" value="org.girlscouts.vtk.modifiedcheck.impl.ModifiedCheckerServlet"/>
        <reference name="checker" interface="org.girlscouts.vtk.modifiedcheck.ModifiedChecker" cardinality="1..1" policy="static" bind="bindChecker" unbind="unbindChecker"/>
    </scr:component>
</components>
