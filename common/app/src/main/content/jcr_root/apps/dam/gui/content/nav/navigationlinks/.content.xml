        <brochures-templates jcr:primaryType="nt:unstructured">
        	<granite:rendercondition
	            jcr:primaryType="nt:unstructured"
	            sling:resourceType="granite/ui/components/foundation/renderconditions/privilege"
				path="/etc/renderconditions/features/content/assets/navigationlinks"
		        privileges="[jcr:read]"/>
        </brochures-templates>
        <catalogs jcr:primaryType="nt:unstructured">
        	<granite:rendercondition
	            jcr:primaryType="nt:unstructured"
	            sling:resourceType="granite/ui/components/foundation/renderconditions/privilege"
				path="/etc/renderconditions/features/content/assets/navigationlinks"
		        privileges="[jcr:read]"/>
        </catalogs>
