<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content jcr:primaryType="nt:unstructured">
        <actions jcr:primaryType="nt:unstructured">
            <selection jcr:primaryType="nt:unstructured">
                <downloadcf jcr:primaryType="nt:unstructured" sling:hideResource="true"/>
                <extractarchive jcr:primaryType="nt:unstructured" sling:hideResource="true"/>
                <share jcr:primaryType="nt:unstructured" sling:hideResource="true"/>
                <desktop jcr:primaryType="nt:unstructured" sling:hideResource="true"/>
                <checkedoutenabled jcr:primaryType="nt:unstructured" sling:hideResource="true"/>
                <checkedoutdisabled jcr:primaryType="nt:unstructured" sling:hideResource="true"/>
                <checkout jcr:primaryType="nt:unstructured" sling:hideResource="true"/>
                <checkin jcr:primaryType="nt:unstructured" sling:hideResource="true"/>
                <viewfragmentprops jcr:primaryType="nt:unstructured" sling:hideResource="true"/>
                <foldershare jcr:primaryType="nt:unstructured" sling:hideResource="true"/>
                <annotate jcr:primaryType="nt:unstructured" sling:hideResource="true"/>
                <addtocollection jcr:primaryType="nt:unstructured" sling:hideResource="true"/>
                <createtask jcr:primaryType="nt:unstructured" sling:hideResource="true"/>
                <publish jcr:primaryType="nt:unstructured">
                    <items jcr:primaryType="nt:unstructured">
                        <cancelscheduledactivation		                    
		                    granite:rel="cq-siteadmin-admin-actions-cancel-scheduled-workflow-activator"
		                    relScope="none"
		                    jcr:primaryType="nt:unstructured"
		                    sling:resourceType="granite/ui/components/coral/foundation/collection/actionlink"
		                    activeSelectionCount="single"		                                       
		                    text="Cancel Scheduled Activations">
		                    <granite:data
		                        jcr:primaryType="nt:unstructured"                        
		                        workflow-type="activation"/>
		                    <granite:rendercondition
		                        jcr:primaryType="nt:unstructured"
		                        sling:resourceType="granite/ui/components/foundation/renderconditions/and">
		                        <privilege
		                            jcr:primaryType="nt:unstructured"
		                            sling:resourceType="granite/ui/components/foundation/renderconditions/privilege"
		                            path="/content/girlscouts-template"
		                            privileges="[crx:replicate]"/>
		                        <mainasset
		                            jcr:primaryType="nt:unstructured"
		                            sling:resourceType="dam/gui/coral/components/commons/renderconditions/mainasset"/>
		                    </granite:rendercondition>
		                </cancelscheduledactivation>
                    </items>
                </publish>
                <unpublish jcr:primaryType="nt:unstructured">
                    <items jcr:primaryType="nt:unstructured">
                        <cancelscheduleddeactivation		                    
		                    granite:rel="cq-siteadmin-admin-actions-cancel-scheduled-workflow-activator"
		                    relScope="none"
		                    jcr:primaryType="nt:unstructured"
		                    sling:resourceType="granite/ui/components/coral/foundation/collection/actionlink"
		                    activeSelectionCount="single"                                       
		                    text="Cancel Scheduled Deactivations">	
		                    <granite:data
		                        jcr:primaryType="nt:unstructured"                        
		                        workflow-type="deactivation"/>	                    
		                    <granite:rendercondition
		                        jcr:primaryType="nt:unstructured"
		                        sling:resourceType="granite/ui/components/foundation/renderconditions/and">
		                        <privilege
		                            jcr:primaryType="nt:unstructured"
		                            sling:resourceType="granite/ui/components/foundation/renderconditions/privilege"
		                            path="/content/girlscouts-template"
		                            privileges="[crx:replicate]"/>
		                        <mainasset
		                            jcr:primaryType="nt:unstructured"
		                            sling:resourceType="dam/gui/coral/components/commons/renderconditions/mainasset"/>
		                    </granite:rendercondition>
		                </cancelscheduleddeactivation>
                    </items>
                </unpublish>
                <downloadreferencesreport
                    granite:rel="cq-damadmin-admin-actions-download-references-report-activator"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/collection/action"
                    activeSelectionCount="multiple"
                    icon="download"
                    relScope="none"
                    target=".cq-damadmin-admin-childpages"
                    text="Download References Report"
                    variant="actionBar">
                    <granite:data
                        jcr:primaryType="nt:unstructured"                        
                        href="/etc/servlets/assetreferences.html"/> 
                    <granite:rendercondition
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/renderconditions/and">
                        <privilege
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/renderconditions/privilege"
                            path="/content/girlscouts-template"
                            privileges="[crx:replicate]"/>
                        <mainasset
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="dam/gui/coral/components/commons/renderconditions/mainasset"/>
                    </granite:rendercondition>                   
                </downloadreferencesreport>                  
                <relate jcr:primaryType="nt:unstructured" sling:hideResource="true"/>
                <unrelate jcr:primaryType="nt:unstructured" sling:hideResource="true"/>
            </selection>
        </actions>
    </jcr:content>
</jcr:root>
