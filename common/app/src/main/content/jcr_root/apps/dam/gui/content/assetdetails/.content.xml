<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content jcr:primaryType="nt:unstructured">
        <actions jcr:primaryType="nt:unstructured">
            <share jcr:primaryType="nt:unstructured">
                <granite:rendercondition
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/renderconditions/and">
                    <privilege
				            jcr:primaryType="nt:unstructured"
				            sling:resourceType="granite/ui/components/foundation/renderconditions/privilege"
							path="/etc/renderconditions/features/content/assets"
					        privileges="[jcr:read]"/>
                    <mainasset
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="dam/gui/coral/components/commons/renderconditions/mainasset"
                        path="${requestPathInfo.suffix}"/>
                    <notexpiredorisadmin
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="/libs/dam/gui/components/admin/renderconditions/isviewablebycurrentuserandhasnotexpired"
                        path="${requestPathInfo.suffix}"/>
                </granite:rendercondition>
            </share>
            <desktop jcr:primaryType="nt:unstructured">
                <granite:rendercondition
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/renderconditions/and">
                    <privilege
				            jcr:primaryType="nt:unstructured"
				            sling:resourceType="granite/ui/components/foundation/renderconditions/privilege"
							path="/etc/renderconditions/features/content/assets"
					        privileges="[jcr:read]"/>
	                <userprop
	                    jcr:primaryType="nt:unstructured"
	                    sling:resourceType="dam/gui/coral/components/admin/renderconditions/userprop"
	                    property="preferences/showAssetDesktopLinks"
	                    value="true"/>
	        	</granite:rendercondition>
            </desktop>
            <checkedoutenabled jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/button">                    
                <granite:rendercondition
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/renderconditions/and">
                    <privilege
				            jcr:primaryType="nt:unstructured"
				            sling:resourceType="granite/ui/components/foundation/renderconditions/privilege"
							path="/etc/renderconditions/features/content/assets"
					        privileges="[jcr:read]"/>
                    <not
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/renderconditions/not">
                        <ischeckedoutbycurrentuser
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="/libs/dam/gui/components/admin/renderconditions/ischeckedoutbycurrentuser"/>
                    </not>
                    <mainasset
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="dam/gui/coral/components/commons/renderconditions/mainasset"/>
                    <cancheckin
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="/libs/dam/gui/components/admin/renderconditions/cancheckin"/>
                </granite:rendercondition>
            </checkedoutenabled>
            <checkedoutdisabled jcr:primaryType="nt:unstructured">
                <granite:rendercondition
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/renderconditions/and">
                    <privilege
				            jcr:primaryType="nt:unstructured"
				            sling:resourceType="granite/ui/components/foundation/renderconditions/privilege"
							path="/etc/renderconditions/features/content/assets"
					        privileges="[jcr:read]"/>
                    <ischeckedout
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="/libs/dam/gui/components/admin/renderconditions/ischeckedout"/>
                    <not
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/renderconditions/not">
                        <cancheckin
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="/libs/dam/gui/components/admin/renderconditions/cancheckin"/>
                    </not>
                    <mainasset
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="dam/gui/coral/components/commons/renderconditions/mainasset"/>
                </granite:rendercondition>
            </checkedoutdisabled>
            <checkout jcr:primaryType="nt:unstructured">
                <granite:rendercondition
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/renderconditions/and">
                    <privilege
				            jcr:primaryType="nt:unstructured"
				            sling:resourceType="granite/ui/components/foundation/renderconditions/privilege"
							path="/etc/renderconditions/features/content/assets"
					        privileges="[jcr:read]"/>
                    <cancheckout
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="/libs/dam/gui/components/admin/renderconditions/cancheckout"/>
                    <mainasset
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="dam/gui/coral/components/commons/renderconditions/mainasset"/>
                </granite:rendercondition>
                <granite:data
                    jcr:primaryType="nt:unstructured"
                    path="${requestPathInfo.suffix}"/>
            </checkout>
            <checkin jcr:primaryType="nt:unstructured">
                <granite:rendercondition
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/renderconditions/and">
                    <privilege
			            jcr:primaryType="nt:unstructured"
			            sling:resourceType="granite/ui/components/foundation/renderconditions/privilege"
						path="/etc/renderconditions/features/content/assets"
				        privileges="[jcr:read]"/>
                    <ischeckedoutbycurrentuser
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="/libs/dam/gui/components/admin/renderconditions/ischeckedoutbycurrentuser"/>
                    <cancheckin
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="/libs/dam/gui/components/admin/renderconditions/cancheckin"/>
                    <mainasset
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="dam/gui/coral/components/commons/renderconditions/mainasset"/>
                </granite:rendercondition>
            </checkin>
            <annotate jcr:primaryType="nt:unstructured">
                <granite:rendercondition
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/renderconditions/and">
                    <privilege
				            jcr:primaryType="nt:unstructured"
				            sling:resourceType="granite/ui/components/foundation/renderconditions/privilege"
							path="/etc/renderconditions/features/content/assets"
					        privileges="[jcr:read]"/>
                    <subasset
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/renderconditions/or">
                        <mainasset
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="dam/gui/coral/components/commons/renderconditions/mainasset"/>
                        <parentassetnotcheckedout
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/renderconditions/not">
                            <isparentassetcheckedout
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="/libs/dam/gui/components/admin/renderconditions/isparentassetcheckedout"/>
                        </parentassetnotcheckedout>
                        <isparentassetcheckedoutbycurrentuser
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="/libs/dam/gui/components/admin/renderconditions/isparentassetcheckedoutbycurrentuser"/>
                    </subasset>
                    <checkout
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/renderconditions/or">
                        <notcheckedout
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/renderconditions/not">
                            <ischeckedout
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="/libs/dam/gui/components/admin/renderconditions/ischeckedout"/>
                        </notcheckedout>
                        <ischeckedoutbycurrentuser
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="/libs/dam/gui/components/admin/renderconditions/ischeckedoutbycurrentuser"/>
                    </checkout>
                </granite:rendercondition>
            </annotate>
            <svedit jcr:primaryType="nt:unstructured">
                <granite:rendercondition
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/renderconditions/and">
                    <privilege
				            jcr:primaryType="nt:unstructured"
				            sling:resourceType="granite/ui/components/foundation/renderconditions/privilege"
							path="/etc/renderconditions/features/content/assets"
					        privileges="[jcr:read]"/>
                    <subassetcondition
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/renderconditions/or">
                        <mainasset
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="dam/gui/coral/components/commons/renderconditions/mainasset"/>
                        <parentassetnotcheckedout
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/renderconditions/not">
                            <isparentassetcheckedout
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="/libs/dam/gui/components/admin/renderconditions/isparentassetcheckedout"/>
                        </parentassetnotcheckedout>
                        <isparentassetcheckedoutbycurrentuser
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="/libs/dam/gui/components/admin/renderconditions/isparentassetcheckedoutbycurrentuser"/>
                    </subassetcondition>
                    <checkoutcondition
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/renderconditions/or">
                        <notcheckedout
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/renderconditions/not">
                            <ischeckedout
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="/libs/dam/gui/components/admin/renderconditions/ischeckedout"/>
                        </notcheckedout>
                        <ischeckedoutbycurrentuser
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="/libs/dam/gui/components/admin/renderconditions/ischeckedoutbycurrentuser"/>
                    </checkoutcondition>
                    <dynamicmediacondition
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/renderconditions/or">
                        <dynamicmediaenabled
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/renderconditions/and">
                            <dynamicmedia
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/renderconditions/feature"
                                feature="com.adobe.dam.asset.dynamicmedia.feature.flag"/>
                            <validateassettype
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="dam/gui/components/s7dam/dmrenderconditions/assettyperendercondition"
                                validTypes="[DMVideo]"/>
                        </dynamicmediaenabled>
                    </dynamicmediacondition>
                </granite:rendercondition>
            </svedit>
            <addtocollection jcr:primaryType="nt:unstructured">
	            <granite:rendercondition
	                    jcr:primaryType="nt:unstructured"
	                    sling:resourceType="granite/ui/components/coral/foundation/renderconditions/and">
	            	<privilege
			            jcr:primaryType="nt:unstructured"
			            sling:resourceType="granite/ui/components/foundation/renderconditions/privilege"
						path="/etc/renderconditions/features/content/assets"
				        privileges="[jcr:read]"/>
	                <mainasset
	                    jcr:primaryType="nt:unstructured"
	                    sling:resourceType="dam/gui/coral/components/commons/renderconditions/mainasset"/>
            	</granite:rendercondition>
            </addtocollection>
            <deleterendition jcr:primaryType="nt:unstructured">
                <granite:rendercondition
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/renderconditions/and">
                    <privilege
			            jcr:primaryType="nt:unstructured"
			            sling:resourceType="granite/ui/components/foundation/renderconditions/privilege"
						path="/etc/renderconditions/features/content/assets"
				        privileges="[jcr:read]"/>
                    <subasset
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/renderconditions/or">
                        <mainasset
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="dam/gui/coral/components/commons/renderconditions/mainasset"/>
                        <parentassetnotcheckedout
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/renderconditions/not">
                            <isparentassetcheckedout
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="/libs/dam/gui/components/admin/renderconditions/isparentassetcheckedout"/>
                        </parentassetnotcheckedout>
                        <isparentassetcheckedoutbycurrentuser
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="/libs/dam/gui/components/admin/renderconditions/isparentassetcheckedoutbycurrentuser"/>
                    </subasset>
                    <checkout
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/renderconditions/or">
                        <notcheckedout
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/renderconditions/not">
                            <ischeckedout
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="/libs/dam/gui/components/admin/renderconditions/ischeckedout"/>
                        </notcheckedout>
                        <ischeckedoutbycurrentuser
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="/libs/dam/gui/components/admin/renderconditions/ischeckedoutbycurrentuser"/>
                    </checkout>
                </granite:rendercondition>
                <granite:data
                    jcr:primaryType="nt:unstructured"
                    foundation-toggleable-control-src="/mnt/overlay/dam/gui/content/assetdetails/modals/deleterendition.html"/>
            </deleterendition>
            <printasset jcr:primaryType="nt:unstructured">     
	            <granite:rendercondition
	                    jcr:primaryType="nt:unstructured"
	                    sling:resourceType="granite/ui/components/coral/foundation/renderconditions/and">       
	            	<privilege
			            jcr:primaryType="nt:unstructured"
			            sling:resourceType="granite/ui/components/foundation/renderconditions/privilege"
						path="/etc/renderconditions/features/content/assets"
				        privileges="[jcr:read]"/>    
	                <orCondition
	                    jcr:primaryType="nt:unstructured"
	                    sling:resourceType="granite/ui/components/coral/foundation/renderconditions/or">
	                    <hasSubAssets
	                        jcr:primaryType="nt:unstructured"
	                        sling:resourceType="dam/gui/coral/components/commons/renderconditions/hassubassets"
	                        path="${requestPathInfo.suffix}"/>
	                    <hasAnnotations
	                        jcr:primaryType="nt:unstructured"
	                        sling:resourceType="dam/gui/coral/components/commons/renderconditions/hasannotations"
	                        path="${requestPathInfo.suffix}"/>
	                    <hasReviewStatus
	                        jcr:primaryType="nt:unstructured"
	                        sling:resourceType="dam/gui/coral/components/commons/renderconditions/hasreviewstatus"
	                        path="${requestPathInfo.suffix}"/>
	                </orCondition>
                </granite:rendercondition>
            </printasset>
        </actions>
    </jcr:content>
</jcr:root>
