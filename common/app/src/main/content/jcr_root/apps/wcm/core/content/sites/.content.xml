<?xml version="1.0" encoding="UTF-8"?>
<jcr:root
        xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
        xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
        xmlns:cq="http://www.day.com/jcr/cq/1.0"
        xmlns:jcr="http://www.jcp.org/jcr/1.0"
        xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
        jcr:primaryType="cq:Page">
    <jcr:content jcr:primaryType="nt:unstructured">
        <actions jcr:primaryType="nt:unstructured">
            <secondary jcr:primaryType="nt:unstructured">
                <create
                    jcr:primaryType="nt:unstructured">
                    <items jcr:primaryType="nt:unstructured">
                    	<createsite jcr:primaryType="nt:unstructured">
                    		<granite:rendercondition
					            jcr:primaryType="nt:unstructured"
					            sling:resourceType="granite/ui/components/foundation/renderconditions/privilege"
								path="/etc/renderconditions/features/content/sites/createlinks"
						        privileges="[jcr:read]"/>
                    	</createsite>
                        <createlivecopy jcr:primaryType="nt:unstructured">
                        	<granite:rendercondition
					            jcr:primaryType="nt:unstructured"
					            sling:resourceType="granite/ui/components/foundation/renderconditions/privilege"
								path="/etc/renderconditions/features/content/sites/livecopylinks"
						        privileges="[jcr:read]"/>
                        </createlivecopy>
                        <createlaunch jcr:primaryType="nt:unstructured">
                        	<granite:rendercondition
					            jcr:primaryType="nt:unstructured"
					            sling:resourceType="granite/ui/components/foundation/renderconditions/privilege"
								path="/etc/renderconditions/features/content/sites/createlinks"
						        privileges="[jcr:read]"/>
						</createlaunch>
                        <createlanguagecopy jcr:primaryType="nt:unstructured">
                        	<granite:rendercondition
		                        jcr:primaryType="nt:unstructured"
		                        sling:resourceType="granite/ui/components/foundation/renderconditions/and">
	                        	<privilege
						            jcr:primaryType="nt:unstructured"
						            sling:resourceType="granite/ui/components/foundation/renderconditions/privilege"
									path="/etc/renderconditions/features/content/sites/createlinks"
							        privileges="[jcr:read]"/>
	                            <languagecopyrendercondition
	                                jcr:primaryType="nt:unstructured"
	                                sling:resourceType="cq/translation/languagecopyrendercondition"/>
                           	</granite:rendercondition>
                        </createlanguagecopy>                        
                        <createfolder jcr:primaryType="nt:unstructured">
                    		<granite:rendercondition
					            jcr:primaryType="nt:unstructured"
					            sling:resourceType="granite/ui/components/foundation/renderconditions/privilege"
								path="/etc/renderconditions/features/content/sites/createlinks"
						        privileges="[jcr:read]"/>
						</createfolder>
                        <scaffolding
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="wcm/core/components/scaffolding-actions"
                            action="foundation.link"
                            activeCondition="site.scaffolding"
                            relScope="collection"/>
                        <bulkeditor
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="wcm/core/components/bulkeditor-actions"
                            action="foundation.link"
                            activeCondition="site.bulkeditor"
                            relScope="collection">
                            <granite:rendercondition
					            jcr:primaryType="nt:unstructured"
					            sling:resourceType="granite/ui/components/foundation/renderconditions/privilege"
								path="/etc/renderconditions/features/content/sites/createlinks"
						        privileges="[jcr:read]"/>
                        </bulkeditor>
                        <createcatalog jcr:primaryType="nt:unstructured">
                        	<granite:rendercondition
					            jcr:primaryType="nt:unstructured"
					            sling:resourceType="granite/ui/components/foundation/renderconditions/privilege"
								path="/etc/renderconditions/features/content/sites/createlinks"
						        privileges="[jcr:read]"/>
                        </createcatalog>
                        <createcsvexport jcr:primaryType="nt:unstructured">
                        	<granite:rendercondition
					            jcr:primaryType="nt:unstructured"
					            sling:resourceType="granite/ui/components/foundation/renderconditions/privilege"
								path="/etc/renderconditions/features/content/sites/createlinks"
						        privileges="[jcr:read]"/>
                        </createcsvexport>
                    </items>
                </create>
            </secondary>
            <selection jcr:primaryType="nt:unstructured">
                <create jcr:primaryType="nt:unstructured">
                    <items jcr:primaryType="nt:unstructured">
                        <createworkflow jcr:primaryType="nt:unstructured">
                            <granite:rendercondition
					            jcr:primaryType="nt:unstructured"
					            sling:resourceType="granite/ui/components/foundation/renderconditions/privilege"
								path="/etc/renderconditions/features/content/sites/createlinks"
						        privileges="[jcr:read]"/>
                            <granite:rendercondition
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/renderconditions/canreadworkflowmodels"/>                            
                        </createworkflow>
                        <createversion jcr:primaryType="nt:unstructured">
                            <granite:rendercondition
					            jcr:primaryType="nt:unstructured"
					            sling:resourceType="granite/ui/components/foundation/renderconditions/privilege"
								path="/etc/renderconditions/features/content/sites/createlinks"
						        privileges="[jcr:read]"/>                            
                        </createversion>
                        <createsite jcr:primaryType="nt:unstructured">
                            <granite:rendercondition
					            jcr:primaryType="nt:unstructured"
					            sling:resourceType="granite/ui/components/foundation/renderconditions/privilege"
								path="/etc/renderconditions/features/content/sites/createlinks"
						        privileges="[jcr:read]"/>                            
                        </createsite>
                        <createlivecopy jcr:primaryType="nt:unstructured">
                            <granite:rendercondition
					            jcr:primaryType="nt:unstructured"
					            sling:resourceType="granite/ui/components/foundation/renderconditions/privilege"
								path="/etc/renderconditions/features/content/sites/createlinks"
						        privileges="[jcr:read]"/>                            
                        </createlivecopy>
                        <createlaunch jcr:primaryType="nt:unstructured">
                            <granite:rendercondition
					            jcr:primaryType="nt:unstructured"
					            sling:resourceType="granite/ui/components/foundation/renderconditions/privilege"
								path="/etc/renderconditions/features/content/sites/createlinks"
						        privileges="[jcr:read]"/>                            
                        </createlaunch>
                        <createlanguagecopy jcr:primaryType="nt:unstructured">
                            <granite:rendercondition
					            jcr:primaryType="nt:unstructured"
					            sling:resourceType="granite/ui/components/foundation/renderconditions/privilege"
								path="/etc/renderconditions/features/content/sites/createlinks"
						        privileges="[jcr:read]"/>                            
                            <granite:rendercondition
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/translation/languagecopyrendercondition"/>
                        </createlanguagecopy>
                        <createcatalog jcr:primaryType="nt:unstructured">
                            <granite:rendercondition
					            jcr:primaryType="nt:unstructured"
					            sling:resourceType="granite/ui/components/foundation/renderconditions/privilege"
								path="/etc/renderconditions/features/content/sites/createlinks"
						        privileges="[jcr:read]"/>                           
                        </createcatalog>
                        <createfolder jcr:primaryType="nt:unstructured">
                            <granite:rendercondition
					            jcr:primaryType="nt:unstructured"
					            sling:resourceType="granite/ui/components/foundation/renderconditions/privilege"
								path="/etc/renderconditions/features/content/sites/createlinks"
						        privileges="[jcr:read]"/>                            
                        </createfolder>
                        <createcsvexport jcr:primaryType="nt:unstructured">
                            <granite:rendercondition
					            jcr:primaryType="nt:unstructured"
					            sling:resourceType="granite/ui/components/foundation/renderconditions/privilege"
								path="/etc/renderconditions/features/content/sites/createlinks"
						        privileges="[jcr:read]"/>                            
                        </createcsvexport>
                    </items>
                </create>                
                <viewfolderproperties jcr:primaryType="nt:unstructured">
                		<granite:rendercondition
				            jcr:primaryType="nt:unstructured"
				            sling:resourceType="granite/ui/components/foundation/renderconditions/privilege"
							path="/etc/renderconditions/features/content/sites/createlinks"
					        privileges="[jcr:read]"/>
                </viewfolderproperties>
                <openconteniInsight jcr:primaryType="nt:unstructured">
                	<granite:rendercondition
					            jcr:primaryType="nt:unstructured"
					            sling:resourceType="granite/ui/components/foundation/renderconditions/privilege"
								path="/etc/renderconditions/features/content/sites/createlinks"
						        privileges="[jcr:read]"/>
                </openconteniInsight>
                <cancelscheduledactivation
                    granite:rel="cq-siteadmin-admin-actions-cancel-scheduled-workflow-activator"
                    relScope="none"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/collection/action"
                    activeSelectionCount="single"
                    icon="globeStrikeClock"
                    target=".cq-siteadmin-admin-childpages"
                    text="Cancel Scheduled Publish"
                    variant="actionBar">
                    <granite:data
                       jcr:primaryType="nt:unstructured"                        
                       workflow-type="activation"/>	                    
                   	<granite:rendercondition
                       jcr:primaryType="nt:unstructured"
                       sling:resourceType="granite/ui/components/foundation/renderconditions/and">
                       <privilege
                          	jcr:primaryType="nt:unstructured"
                          	sling:resourceType="granite/ui/components/foundation/renderconditions/privilege"
                          	path="${requestPathInfo.suffix}"
				          	privileges="[crx:replicate]"/>
                       <mainasset
                           	jcr:primaryType="nt:unstructured"
                           	sling:resourceType="dam/gui/coral/components/commons/renderconditions/mainasset"/>
                   	</granite:rendercondition>
                </cancelscheduledactivation>
                <cancelscheduleddeactivation
                    granite:rel="cq-siteadmin-admin-actions-cancel-scheduled-workflow-activator"
                    relScope="none"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/collection/action"
                    activeSelectionCount="single"
                    icon="globeStrikeClock"
                    target=".cq-siteadmin-admin-childpages"
                    text="Cancel Scheduled Unpublish"
                    variant="actionBar">
                    <granite:data
                        jcr:primaryType="nt:unstructured"                        
                        workflow-type="deactivation"/>	                    
                   	<granite:rendercondition
                       jcr:primaryType="nt:unstructured"
                       sling:resourceType="granite/ui/components/foundation/renderconditions/and">
                       <privilege
                           	jcr:primaryType="nt:unstructured"
                           	sling:resourceType="granite/ui/components/foundation/renderconditions/privilege"
                           	path="${requestPathInfo.suffix}"
				        	privileges="[crx:replicate]"/>
                       <mainasset
                           jcr:primaryType="nt:unstructured"
                           sling:resourceType="dam/gui/coral/components/commons/renderconditions/mainasset"/>
                   	</granite:rendercondition>
                </cancelscheduleddeactivation>
                <downloadreferencesreport
                    granite:rel="cq-siteadmin-admin-actions-download-references-report-activator"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/collection/action"
                    activeSelectionCount="multiple"
                    icon="download"
                    relScope="none"
                    target=".cq-siteadmin-admin-childpages"
                    text="Download References Report"
                    variant="actionBar">
                    <granite:data
                        jcr:primaryType="nt:unstructured"                        
                        href="/etc/servlets/pagereferences.html"/> 
                    <granite:rendercondition
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/renderconditions/and">
                        <privilege
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/renderconditions/privilege"
                            path="/etc/renderconditions/features/referencesreport"
				        	privileges="[jcr:read]"/>
                        <mainasset
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="dam/gui/coral/components/commons/renderconditions/mainasset"/>
                    </granite:rendercondition>                   
                </downloadreferencesreport> 
            </selection>
        </actions>
    </jcr:content>
    <createlivecopywizard/>
</jcr:root>
