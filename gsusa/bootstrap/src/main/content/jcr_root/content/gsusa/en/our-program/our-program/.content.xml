                        sling:resourceType="girlscouts/components/textimage"
                        additionalCss="nowrap nopadding"
                        text="&lt;p>&lt;b>Take Action.&lt;/b>&lt;br>&#xa;Do&lt;i> &lt;/i>something to make the world a better place.&lt;br>&#xa;&amp;nbsp;&lt;/p>&#xa;"
                        textIsRich="true">
                        <image
