                        jcr:lastModified="{Date}2015-07-08T19:00:35.080-04:00"
                        jcr:lastModifiedBy="mkaufman"
                        jcr:mixinTypes="[cq:LiveRelationship]"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="girlscouts/components/text"
                        text="&lt;h4>&lt;b>Our Mission&lt;/b>&lt;/h4>&#xa;&lt;p>Girl Scouting builds girls of courage, confidence, and character, who make the world a better place.&lt;br>&#xa;&lt;br>&#xa;&lt;/p>&#xa;"
                        textIsRich="true"/>
                    <text_2
                        cq:lastRolledout="{Date}2015-07-08T19:00:35.081-04:00"
                        cq:lastRolledoutBy="admin"
                        jcr:created="{Date}2014-07-08T16:17:30.480-04:00"
                        jcr:createdBy="mkaufman"
                        jcr:lastModified="{Date}2015-07-08T19:00:35.081-04:00"
                        jcr:lastModifiedBy="mkaufman"
                        jcr:mixinTypes="[cq:LiveRelationship]"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="girlscouts/components/text"
                        text="&lt;h4>&lt;b>Girl Scout Promise&amp;nbsp;&lt;/b>&lt;/h4>&#xa;&lt;p>&lt;i>On my honor, I will try:&lt;/i>&lt;br>&#xa;&amp;nbsp; &amp;nbsp; To serve God and my country,&lt;br>&#xa;&amp;nbsp; &amp;nbsp; To help people at all times,&amp;nbsp;&lt;br>&#xa;&amp;nbsp; &amp;nbsp; And to live by the Girl Scout Law.&lt;br>&#xa;&amp;nbsp;&lt;/p>&#xa;"
                        textIsRich="true"/>
                    <text_3
                        cq:lastRolledout="{Date}2015-07-08T19:00:35.082-04:00"
                        cq:lastRolledoutBy="admin"
                        jcr:created="{Date}2014-07-08T16:22:16.003-04:00"
                        jcr:createdBy="mkaufman"
                        jcr:lastModified="{Date}2015-07-08T19:00:35.082-04:00"
                        jcr:lastModifiedBy="mkaufman"
                        jcr:mixinTypes="[cq:LiveRelationship]"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="girlscouts/components/text"
                        text="&lt;h4>&lt;b>Girl Scout Law&lt;/b>&lt;/h4>&#xa;&lt;p>&lt;i>I will do my best to be&lt;br>&#xa; &lt;/i>&amp;nbsp; &amp;nbsp; honest and fair,&lt;br>&#xa;&amp;nbsp; &amp;nbsp; friendly and helpful,&lt;br>&#xa;&amp;nbsp; &amp;nbsp; considerate and caring,&amp;nbsp;&lt;br>&#xa;&amp;nbsp; &amp;nbsp; courageous and strong, and&lt;br>&#xa;&amp;nbsp; &amp;nbsp; responsible for what I say and do,&amp;nbsp;&lt;br>&#xa;&lt;i>and to&amp;nbsp;&lt;/i>&lt;br>&#xa;&amp;nbsp; &amp;nbsp; respect myself and others,&amp;nbsp;&lt;br>&#xa;&amp;nbsp; &amp;nbsp; respect authority,&lt;br>&#xa;&amp;nbsp; &amp;nbsp; use resources wisely,&lt;br>&#xa;&amp;nbsp; &amp;nbsp; make the world a better place, and&amp;nbsp;&lt;br>&#xa;&amp;nbsp; &amp;nbsp; be a sister to every Girl Scout.&amp;nbsp;&lt;br>&#xa;&lt;i>&amp;nbsp;&lt;/i>&lt;/p>&#xa;"
                        textIsRich="true"/>
