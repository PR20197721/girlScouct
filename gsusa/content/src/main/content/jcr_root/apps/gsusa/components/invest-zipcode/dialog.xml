<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <properties
                jcr:primaryType="cq:Panel"
                title="Properties">
                <items jcr:primaryType="cq:WidgetCollection">
                	<local
                		jcr:primaryType="cq:Widget"
                		fieldLabel="Display Local Invest Option - By Zipcode"
                		name="./local"
                		type="checkbox"
                		xtype="selection"/>
                	<national
                		jcr:primaryType="cq:Widget"
                		fieldLabel="Display National Invest Option"
                		name="./national"
                		type="checkbox"
                		xtype="selection"/>
                	<link
                		jcr:primaryType="cq:Widget"
                		fieldLabel="Enter the URL for the national option"
                		name="./link"
                		linkPattern="\{0\}.html"
                		xtype="pathfield"/>
                </items>
            </properties>
        </items>
    </items>
</jcr:root>
