<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <properties
                jcr:primaryType="cq:Panel"
                title="Properties">
                <items jcr:primaryType="cq:WidgetCollection">
                    <username
                        jcr:primaryType="cq:Widget"
                        fieldDescription="Enter your username as it appears in the URL on your profile page"
                        fieldLabel="Username"
                        name="./username"
                        xtype="textfield"/>
                    <height
                        jcr:primaryType="cq:Widget"
                        fieldDescription="Leave blank to use default"
                        fieldLabel="Height"
                        name="./height"
                        xtype="numberfield"/>
                    <width
                        jcr:primaryType="cq:Widget"
                        fieldDescription="Leave blank to use default"
                        fieldLabel="Width"
                        name="./width"
                        xtype="numberfield"/>
                    <tweet-limit
                        jcr:primaryType="cq:Widget"
                        fieldDescription="(Optional) Specify a limit for how many tweets will appear"
                        fieldLabel="Tweet Limit"
                        name="./tweetLimit"
                        xtype="numberfield"/>
                </items>
            </properties>
        </items>
    </items>
</jcr:root>
