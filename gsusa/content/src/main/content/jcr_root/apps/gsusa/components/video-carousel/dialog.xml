                		fieldLabel="Video 1"
                		name="./vid1"
                		xtype="pathfield"/>
                	<path2
                		jcr:primaryType="cq:Widget"
                		fieldLabel="Video 2"
                		name="./vid2"
                		xtype="pathfield"/>
                	<path3
                		jcr:primaryType="cq:Widget"
                		fieldLabel="Video 3"
                		name="./vid3"
                		xtype="pathfield"/>
                	<path4
                		jcr:primaryType="cq:Widget"
                		fieldLabel="Video 4"
                		name="./vid4"
                		xtype="pathfield"/>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>
