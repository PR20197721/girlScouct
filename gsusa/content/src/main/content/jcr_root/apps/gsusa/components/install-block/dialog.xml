<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    title="Join"
    width="{Long}800"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Install Block Settings">
                <items jcr:primaryType="cq:WidgetCollection">
                    <title
                        jcr:primaryType="cq:Widget"
                        defaultValue="Install"
                        value="Install"
                        fieldLabel="Title"
                        name="./title"
                        xtype="textfield"/>
                    <desc
                        jcr:primaryType="cq:Widget"
                        defaultValue="The official Girl Scout Cookie Finder App"
                        value="The official Girl Scout Cookie Finder App"
                        fieldLabel="Description"
                        name="./description"
                        xtype="textarea"/>
                    <icon
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Icon"
                        xtype="iconpicker"
                        name="./icon"/>
                    <background-color
                        jcr:primaryType="cq:Widget"
                        colors="[FAA61A,F27536,EC008C,AB218E,00AAE5,C4D82E]"
                        fieldLabel="Background Color"
                        name="./bgcolor"
                        xtype="colorfield"/>
                    <apple
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Apple Appstore"
                        name="./apple"
                        xtype="shoplink"/>
                    <google
                    	jcr:primaryType="cq:Widget"
                    	fieldLabel="Google Play"
                    	name="./google"
                    	xtype="shoplink"/>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>
