<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    title="Booth Finder"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Booth Finder">
                <items jcr:primaryType="cq:WidgetCollection">
                    <show-contact-banner-per
                        jcr:primaryType="cq:Widget"
                        fieldDescription="If the council is in path 2, a contact banner is shown every a few booths. This field is to configure the number."
                        fieldLabel="Show Contact Banner Per"
                        xtype="numberfield"/>
                    <num-per-page
                        jcr:primaryType="cq:Widget"
                        fieldDescription="Number of booths shown on each page"
                        fieldLabel="Number Per Page"
                        xtype="numberfield"/>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>
