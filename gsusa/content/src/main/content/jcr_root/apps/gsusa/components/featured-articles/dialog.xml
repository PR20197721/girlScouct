<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    height="{Long}500"
    width="{Long}750"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <properties
                jcr:primaryType="cq:Panel"
                title="Articles">
                <items jcr:primaryType="cq:WidgetCollection">
                    <largeArticle
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Large Article Tile"
                        name="./largeArticle"
                        xtype="pathfield"/>
                    <smallArticle1
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Small Article Tile 1"
                        name="./smallArticle1"
                        xtype="pathfield"/>
                    <smallArticle2
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Small Article Tile 2"
                        name="./smallArticle2"
                        xtype="pathfield"/>
                    <smallArticle3
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Small Article Tile 3"
                        name="./smallArticle3"
                        xtype="pathfield"/>
                    <smallArticle4
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Small Article Tile 4"
                        name="./smallArticle4"
                        xtype="pathfield"/>
                </items>
            </properties>
        </items>
    </items>
</jcr:root>
