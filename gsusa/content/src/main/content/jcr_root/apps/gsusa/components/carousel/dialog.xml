<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    title="Carouse"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Opening Page">
                <items jcr:primaryType="cq:WidgetCollection">
                    <title
                        jcr:primaryType="cq:Widget"
                        defaultValue="Introduce girls to"
                        fieldLabel="Title"
                        name="./title"
                        xtype="textfield"/>
                    <button
                        jcr:primaryType="cq:Widget"
                        defaultValue="Explore Girls Scout"
                        fieldLabel="Button"
                        name="./button"
                        xtype="textfield"/>
                    <content
                        jcr:primaryType="cq:Widget"
                        defaultValue="experiences the girls scout"
                        fieldLabel="Content"
                        name="./content"
                        xtype="textfield"/>
                    <imagePath
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Images"
                        name="./imagePath"
                        xtype="multifield">
                        <fieldConfig
                            jcr:primaryType="cq:Widget"
                            rootPath="/content/dam/girlscouts-gsusa"
                            xtype="pathfield"/>
                    </imagePath>
                </items>
            </tab1>
            <tab2
                jcr:primaryType="cq:Panel"
                title="Second Page">
                <items jcr:primaryType="cq:WidgetCollection">
                    <title2
                        jcr:primaryType="cq:Widget"
                        defaultValue="What do girl scouts do? Stuff like this:"
                        fieldLabel="Title"
                        name="./title2"
                        xtype="textfield"/>
                    <imagePath2
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Images"
                        name="./imagePath2"
                        xtype="multifield">
                        <fieldConfig
                            jcr:primaryType="cq:Widget"
                            rootPath="/content/dam/girlscouts-gsusa"
                            xtype="pathfield"/>
                    </imagePath2>
                    <content2
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Content"
                        name="./content2"
                        xtype="multifield">
                        <fieldConfig
                            jcr:primaryType="cq:Widget"
                            xtype="textfield"/>
                    </content2>
                    <subtitle2
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Subtitle"
                        name="./subtitle2"
                        xtype="multifield">
                        <fieldConfig
                            jcr:primaryType="cq:Widget"
                            xtype="textfield"/>
                    </subtitle2>
                </items>
            </tab2>
        </items>
    </items>
</jcr:root>
