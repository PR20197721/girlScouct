<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <properties
                jcr:primaryType="cq:Panel"
                title="Properties">
                <items jcr:primaryType="cq:WidgetCollection">
                    <count
                        jcr:primaryType="cq:Widget"
                        fieldDescription="Specify a limit for how many images will appear. Will default to 20 if left blank"
                        fieldLabel="Picture Count"
                        minValue="1"
                        name="./count"
                        xtype="numberfield"/>
                    <pin-id
                    	jcr:primaryType="cq:Widget"
                    	fieldDescription="If you would like to pin an image to the top of the feed, enter its post id here."
                    	fieldLabel="Pin ID"
                    	name="./pin-id"
                    	xtype="textfield"/>
                </items>
            </properties>
        </items>
    </items>
</jcr:root>
