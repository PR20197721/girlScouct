<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    title="Girls Story"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Story path">
                <items jcr:primaryType="cq:WidgetCollection">
                    <path
                        jcr:primaryType="cq:Widget"
                        fieldDescription="Please fill up this field"
                        fieldLabel="Story path"
                        fieldSubLabel="Please Choose a path for the story"
                        name="./path"
                        rootPath="/content/gsusa/en/girls-stories"
                        xtype="pathfield"/>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>
