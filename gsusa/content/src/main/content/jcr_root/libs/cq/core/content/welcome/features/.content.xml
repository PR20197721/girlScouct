<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured">
    <crxde
        jcr:description="Develop applications with a web-based IDE"
        jcr:mixinTypes="[sling:Redirect]"
        jcr:primaryType="nt:unstructured"
        jcr:title="CRXDE Lite"
        sling:resourceType="sling:redirect"
        sling:target="/crx/de/"/>
    <packages
        jcr:description="Package and share applications"
        jcr:mixinTypes="[sling:Redirect]"
        jcr:primaryType="nt:unstructured"
        jcr:title="Packages"
        sling:resourceType="sling:redirect"
        sling:target="/crx/packmgr/"/>
    <share
        jcr:description="Download applications from Adobe and the community"
        jcr:mixinTypes="[sling:Redirect]"
        jcr:primaryType="nt:unstructured"
        jcr:title="Package Share"
        sling:resourceType="sling:redirect"
        sling:target="/crx/packageshare/"/>
    <cluster
        jcr:description="Manage clustered deployments"
        jcr:mixinTypes="[sling:Redirect]"
        jcr:primaryType="nt:unstructured"
        jcr:title="Clustering"
        sling:resourceType="sling:redirect"
        sling:target="/libs/granite/cluster/content/admin"/>
    <backup
        jcr:description="Carry out backup tasks"
        jcr:mixinTypes="[sling:Redirect]"
        jcr:primaryType="nt:unstructured"
        jcr:title="Backup"
        sling:resourceType="sling:redirect"
        sling:target="/libs/granite/backup/content/admin"/>
    <config
        jcr:description="Configure and manage the application platform"
        jcr:mixinTypes="[sling:Redirect]"
        jcr:primaryType="nt:unstructured"
        jcr:title="Web Console"
        sling:resourceType="sling:redirect"
        sling:target="/system/console/configMgr"/>
    <statusdump
        jcr:description="Download Configuration Status from Web Console"
        jcr:mixinTypes="[sling:Redirect]"
        jcr:primaryType="nt:unstructured"
        jcr:title="Web Console Status Dump"
        sling:resourceType="sling:redirect"
        sling:target="/libs/crx/core/content/welcome/osgi/statusdump.zip"/>
    <gsusa-tag-editor
        jcr:description="Manage GSUSA Article Hub tags and change category colors"
        jcr:mixinTypes="[sling:Redirect]"
        jcr:primaryType="nt:unstructured"
        jcr:title="GSUSA Article Hub Tag Editor"
        sling:resourceType="sling:redirect"
        sling:status="302"
        sling:target="/apps/gsusa/wcm/components/tag-editor.html"/>
</jcr:root>
