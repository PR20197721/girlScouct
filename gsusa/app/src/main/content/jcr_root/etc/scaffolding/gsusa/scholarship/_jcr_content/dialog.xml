<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    title="Scholarship"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Scholarship">
                <items jcr:primaryType="cq:WidgetCollection">
                    <title
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        fieldDescription="Please input a title"
                        fieldLabel="Title"
                        name="./jcr:content/jcr:title"
                        xtype="textfield"/>
                    <type
                    	jcr:primaryType="cq:Widget"
                    	allowBlank="{Boolean}false"
                    	fieldLabel="Scholarship Type"
                    	name="./jcr:content/sType"
                    	type="select"
                    	value="All"
                    	defaultValue="All"
                    	xtype="selection">
                    	<options jcr:primaryType="cq:WidgetCollection">
                    		<all
                    			jcr:primaryType="nt:unstructured"
                    			text="All"
                    			value="All"/>
                    		<awards
                    			jcr:primaryType="nt:unstructured"
                    			text="Awards"
                    			value="Awards"/>
                    		<college
                    			jcr:primaryType="nt:unstructured"
                    			text="College/University"
                    			value="College/University"/>
                    		<foundations
                    			jcr:primaryType="nt:unstructured"
                    			text="Foundations/Private"
                    			value="Foundations/Private"/>
                    		<council
                    			jcr:primaryType="nt:unstructured"
                    			text="GS Council"
                    			value="GS Council"/>
                    	</options>
                    </type>
                    <sponsor
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        fieldLabel="Sponsor"
                        name="./jcr:content/sponsor"
                        xtype="textfield"/>
                    <website
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        fieldLabel="Website"
                        name="./jcr:content/website"
                        xtype="textfield"/>
                    <city
                        jcr:primaryType="cq:Widget"
                        fieldLabel="City"
                        name="./jcr:content/city"
                        xtype="textfield"/>
                    <state
                        jcr:primaryType="cq:Widget"
                        fieldLabel="State"
                        name="./jcr:content/state"
                        xtype="textfield"/>
                    <awardName1
                    	jcr:primaryType="cq:Widget"
                    	allowBlank="{Boolean}false"
                    	fieldLabel="Award Name 1"
                    	name="./jcr:content/awardName1"
                    	xtype="textfield"/>
                   	<awardContent1
                   		jcr:primaryType="cq:Widget"
                   		allowBlank="{Boolean}false"
                   		fieldLabel="Award Content 1"
                   		name="./jcr:content/awardContent1"
                   		xtype="textarea"/>
                   	<awardReq1
                   		jcr:primaryType="cq:Widget"
                   		allowBlank="{Boolean}false"
                   		fieldLabel="Award Requirements 1"
                   		name="./jcr:content/awardReq1"
                   		xtype="textarea"/>
                   	<awardName2
                    	jcr:primaryType="cq:Widget"
                    	fieldLabel="Award Name 2"
                    	name="./jcr:content/awardName2"
                    	xtype="textfield"/>
                   	<awardContent2
                   		jcr:primaryType="cq:Widget"
                   		fieldLabel="Award Content 2"
                   		name="./jcr:content/awardContent2"
                   		xtype="textarea"/>
                   	<awardReq2
                   		jcr:primaryType="cq:Widget"
                   		fieldLabel="Award Requirements 2"
                   		name="./jcr:content/awardReq2"
                   		xtype="textarea"/>
                   	<awardName3
                    	jcr:primaryType="cq:Widget"
                    	fieldLabel="Award Name 3"
                    	name="./jcr:content/awardName3"
                    	xtype="textfield"/>
                   	<awardContent3
                   		jcr:primaryType="cq:Widget"
                   		fieldLabel="Award Content 3"
                   		name="./jcr:content/awardContent3"
                   		xtype="textarea"/>
                   	<awardReq3
                   		jcr:primaryType="cq:Widget"
                   		fieldLabel="Award Requirements 3"
                   		name="./jcr:content/awardReq3"
                   		xtype="textarea"/>
                   	<awardName4
                    	jcr:primaryType="cq:Widget"
                    	fieldLabel="Award Name 4"
                    	name="./jcr:content/awardName4"
                    	xtype="textfield"/>
                   	<awardContent4
                   		jcr:primaryType="cq:Widget"
                   		fieldLabel="Award Content 4"
                   		name="./jcr:content/awardContent4"
                   		xtype="textarea"/>
                   	<awardReq4
                   		jcr:primaryType="cq:Widget"
                   		fieldLabel="Award Requirements 4"
                   		name="./jcr:content/awardReq4"
                   		xtype="textarea"/>
                   	<contact
                   		jcr:primaryType="cq:Widget"
                   		fieldLabel="Contact"
                   		fieldDescription="Provide a contact here"
                   		name="./jcr:content/contact"
                   		xtype="textfield"/>
                   	<contactTitle
                   		jcr:primaryType="cq:Widget"
                   		fieldLabel="Contact Title"
                   		name="./jcr:content/contactTitle"
                   		xtype="textfield"/>
                    <contactAddr1
                   		jcr:primaryType="cq:Widget"
                   		fieldLabel="Contact Address 1"
                   		name="./jcr:content/contactAddr1"
                   		xtype="textfield"/>
                   	<contactAddr2
                   		jcr:primaryType="cq:Widget"
                   		fieldLabel="Contact Address 2"
                   		name="./jcr:content/contactAddr2"
                   		xtype="textfield"/>
                   	<contactCity
                   		jcr:primaryType="cq:Widget"
                   		fieldLabel="Contact City"
                   		name="./jcr:content/contactCity"
                   		xtype="textfield"/>
                   	<contactState
                   		jcr:primaryType="cq:Widget"
                   		fieldLabel="Contact State"
                   		name="./jcr:content/contactState"
                   		xtype="textfield"/>
                   	<contactZip
                   		jcr:primaryType="cq:Widget"
                   		fieldLabel="Contact Zip"
                   		name="./jcr:content/contactZip"
                   		xtype="textfield"/>
                   	<contactPhone
                   		jcr:primaryType="cq:Widget"
                   		fieldLabel="Contact Phone"
                   		name="./jcr:content/contactPhone"
                   		xtype="textfield"/>
                   	<contactEmail
                   		jcr:primaryType="cq:Widget"
                   		fieldLabel="Contact Email"
                   		name="./jcr:content/contactEmail"
                   		xtype="textfield"/>
                   	<res
                   		jcr:primaryType="cq:Widget"
                   		name="./jcr:content/sling:resourceType"
                   		value="gsusa/components/data-page"
                   		xtype="hidden"/>
                   	<hide
                   		jcr:primaryType="cq:Widget"
                   		name="./jcr:content/hideInNav"
                   		value="true"
                   		xtype="hidden"/>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>
