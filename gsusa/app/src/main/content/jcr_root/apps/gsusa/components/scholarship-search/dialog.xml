<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <properties
                jcr:primaryType="cq:Panel"
                title="Properties">
                <items jcr:primaryType="cq:WidgetCollection">
                    <file-path
                        jcr:primaryType="cq:Widget"
                        fieldDescription="Please enter the path to the folder in which scholarships are stored"
                        fieldLabel="Path to Scholarships Directory"
                        name="./path"
                        xtype="pathfield"/>
                </items>
            </properties>
        </items>
    </items>
</jcr:root>
