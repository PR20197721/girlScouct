<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"
            type="nav"/>
        <items jcr:primaryType="nt:unstructured">
        
            <maincontent
                jcr:primaryType="nt:unstructured"
                jcr:title="Main Content"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <url
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="API url to send email info"
                                fieldLabel="API Endpoint"
                                name="./url"
                                required="false"/>
                            <heading
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription=""
                                fieldLabel="Heading"
                                name="./heading"/>
                            <description
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription=""
                                fieldLabel="Description"
                                name="./description"/>
                            <source
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Originating source in 300 or less characters"
                                fieldLabel="Source"
    							   defaultValue=""
                                name="./source"
                                required="false"/>
                            <emailplaceholder
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription=""
                                fieldLabel="Email Placeholder"
                                name="./emailplaceholder"/>
                            <emailplaceholdermobile
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="For mobile 320px and limit 25 chars"
                                fieldLabel="Email Placeholder Mobile"
                                name="./emailplaceholdermobile"/>
                            <submitbuttontext
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription=""
                                fieldLabel="Submit Text"
                                name="./submitbuttontext"/>
                            <topborder
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                fieldDescription="Check to apply border at top of the component"
                                name="./topborder"
                                text="Top Border"/>
                            <topborder-delete
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/hidden"
                                name="./topborder@Delete"
                                value="{Boolean}true"/>
                            <bottomborder
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                fieldDescription="Check to apply border at top of the component"
                                name="./bottomborder"
                                text="Bottom Border"/>
                            <bottomborder-delete
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/hidden"
                                name="./bottomborder@Delete"
                                value="{Boolean}true"/>
                        </items>
                    </columns>
                </items>
            </maincontent>
            
            <submission
                jcr:primaryType="nt:unstructured"
                jcr:title="Submission"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <errorheading
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription=""
                                fieldLabel="Error Heading"
                                name="./errorheading"/>
                            <errordescription
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription=""
                                fieldLabel="Error Description"
                                name="./errordescription"/>
                            <successheading
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription=""
                                fieldLabel="Success Heading"
                                name="./successheading"/>
                            <successdescription
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription=""
                                fieldLabel="Success Description"
                                name="./successdescription"/>
                            <alertemailexists
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Alert message for when submitted email already exists"
                                fieldLabel="Alert "
                                name="./alertemailexists"/>
                        </items>
                    </columns>
                </items>
            </submission>
            
            <icon-image
                jcr:primaryType="nt:unstructured"
                jcr:title="Icon Image"
                sling:resourceType="granite/ui/components/foundation/container">
            		<layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
               	<items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
						<file
							jcr:primaryType="nt:unstructured"
							sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
							autoStart="{Boolean}false"
							class="cq-droptarget"
							fieldLabel="Image asset"
    						fileNameParameter="./iconimage/image/fileName"
    						fileReferenceParameter="./iconimage/image/fileReference"
							mimeTypes="[image]"
							multiple="{Boolean}false"
    						name="./iconimage/image/file"
    						title="Icon Image"
							uploadUrl="${suffix.path}"
							useHTML5="{Boolean}true"/>
                        </items>
                    </columns>
                </items>
            </icon-image>
 
            <thankyou-image
                jcr:primaryType="nt:unstructured"
                jcr:title="Thankyou Image"
                sling:resourceType="granite/ui/components/foundation/container">
            		<layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
               	<items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
							<file
								jcr:primaryType="nt:unstructured"
								sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
								autoStart="{Boolean}false"
								class="cq-droptarget"
								fieldLabel="Image asset"
	    						fileNameParameter="./thankyouimage/image/fileName"
	    						fileReferenceParameter="./thankyouimage/image/fileReference"
								mimeTypes="[image]"
								multiple="{Boolean}false"
	    						name="./thankyouimage/image/file"
	    						title="Thank You Image"
								uploadUrl="${suffix.path}"
								useHTML5="{Boolean}true"/>
                        </items>
                    </columns>
                </items>
            </thankyou-image>
            
            <sfmc
                jcr:primaryType="nt:unstructured"
                jcr:title="SFMC"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <councilid
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="3 digit Council ID"
                                fieldLabel="Council ID"
                                name="./cid"/>
                            <dataextensionname
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription=""
                                fieldLabel="Data Extension Name"
                                name="./dataextensionname"/>
                            <dataextensionkey
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription=""
                                fieldLabel="Data Extension Key"
                                name="./dataextensionkey"/>
                        </items>
                    </columns>
                </items>
            </sfmc>
        
        
        </items>
    </content>
</jcr:root>
