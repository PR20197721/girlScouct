<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Camp Landing Hero">
                <items jcr:primaryType="cq:WidgetCollection">
                    <text
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Text"
                        fieldDescription="Text above the camp finder field"
                        name="./text"
                        xtype="textarea"/>
                    <result-page
                        jcr:primaryType="cq:Widget"
                        fieldDescription="Result page that will show the camp finder"
                        fieldLabel="Result Page"
                        name="./resultPage"
                        rootPath="/content/gsusa"
                        xtype="pathfield"/>
                    <images
                        jcr:primaryType="cq:Widget"
                        fieldDescription="background image for the camp finder"
                        fieldLabel="Images"
                        name="./images"
                        rootPath="/content/dam"
                        xtype="pathfield"/>
                    <formbgimages
                        jcr:primaryType="cq:Widget"
                        fieldDescription="background image for the form"
                        fieldLabel="Form Background Images"
                        name="./formbgimages"
                        rootPath="/content/dam"
                        xtype="pathfield"/>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>
