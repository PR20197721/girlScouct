<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    title="Get Updates"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Keep In Touch Settings">
                <items jcr:primaryType="cq:WidgetCollection">
                    <label
                        jcr:primaryType="cq:Widget"
                        defaultValue="Get Updates"
                        fieldLabel="Label"
                        name="./label"
                        value="Get Updates"
                        xtype="textfield"/>
                    <desc
                    	jcr:primaryType="cq:Widget"
                    	defaultValue="Sign up for cookie email updates"
                    	fieldLabel="Description"
                    	name="./desc"
                    	value="Sign up for cookie email updates"
                    	xtype="textfield"/>
                 </items>
             </tab1>
        </items>
    </items>
</jcr:root>
