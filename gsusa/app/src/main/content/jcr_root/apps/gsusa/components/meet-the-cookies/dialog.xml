<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    height="{Long}400"
    width="{Long}1300"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Meet The Cookies">
                <items jcr:primaryType="cq:WidgetCollection">
                    <help
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Help"
                        html="For the cookie titles:  &lt;br/> Enter &amp;lt;span&amp;gt;&amp;amp;trade;&amp;lt;/span&amp;gt; for &amp;trade;. &lt;br/>Enter &amp;lt;span&amp;gt;&amp;amp;reg;&amp;lt;/span&amp;gt; for &amp;reg;."
                        xtype="label"/>
                    <cookies
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Cookies"
                        name="./cookies"
                        xtype="meetthecookiesmultifield">
                        <fieldConfig
                            jcr:primaryType="nt:unstructured"
                            xtype="meetthecookies"/>
                    </cookies>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>
