<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <properties
                jcr:primaryType="cq:Panel"
                title="Properties">
                <items jcr:primaryType="cq:WidgetCollection">
                    <fbPage
                        jcr:primaryType="cq:Widget"
                        fieldDescription="Enter the full name (case sensitive) of the facebook page you would like to display. The default is GirlScoutsUSA. If you use a different page, you will need to provide your own access token"
                        fieldLabel="Facebook Page"
                        name="./fbPage"
                        xtype="textfield"/>
                    <token
                        jcr:primaryType="cq:Widget"
                        fieldDescription="Only fill in if you are using a page other than GirlScoutsUSA"
                        fieldLabel="Access Token"
                        name="./token"
                        xtype="textfield"/>
                </items>
            </properties>
        </items>
    </items>
</jcr:root>
